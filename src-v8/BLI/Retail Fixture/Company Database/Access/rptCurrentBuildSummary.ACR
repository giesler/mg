Private Sub Report_NoData(Cancel As Integer)

  MsgBox "There were no records found.", vbExclamation, Me.Caption
  Cancel = True

End Sub

Private Sub Report_Open(Cancel As Integer)
On Error GoTo errHandler

  Dim fC As Form_frptDatesCust, strCrit As String
  Set fC = New Form_frptDatesCust
  fC.Caption = Me.Caption
  fC.lblDate.Caption = "Shipped Date:"
  fC.txtStoreNo.Visible = True
  
  Pause fC

  If fC.mblnCancel Then
    Cancel = True
  Else
    SetSPVar "ShipFromDate", fC.datFrom
    SetSPVar "ShipToDate", fC.datTo
    SetSPVar "SoldName", fC.cmbSoldName
    SetSPVar "StoreNo", fC.txtStoreNo
    strCrit = FormatFromTo(fC.datFrom, fC.datTo)
    If strCrit <> "" Then strCrit = "Ship Date " + strCrit
    If fC.cmbSoldName <> "" Then
      If strCrit <> "" Then strCrit = strCrit + ", "
      strCrit = strCrit + "Customer: " + fC.cmbSoldName
    End If
    If fC.txtStoreNo <> "" Then
      If strCrit <> "" Then strCrit = strCrit + ", "
      strCrit = strCrit + "Store #: " + fC.txtStoreNo
    End If
    Me.lblCriteria.Caption = strCrit
  End If
  
  Set fC = Nothing

Exit Sub
errHandler:
ErrHand Me.Name, "Report_Open"
Exit Sub
End Sub