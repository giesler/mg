Option Compare Database

Private Sub Report_NoData(Cancel As Integer)
  
  MsgBox "There are no open orders for the selected model.", vbInformation
  Cancel = True

End Sub

Private Sub Report_Open(Cancel As Integer)
On Error GoTo errorHandler

  Dim fR As New Form_frptEnterModel
  If mbTomCat Then
    Me.Caption = "TC Orders: Prep Sheet"
    Me.txtHeading.ControlSource = "=""Tom Cat Model "" & [Model] & "" Prep Sheet"""
    iType = 1
  Else
    Me.Caption = "FC Orders: Prep Sheet"
    Me.txtHeading.ControlSource = "=""Factory Cat Model "" & [Model] & "" Prep Sheet"""
    iType = 0
  End If
  fR.Caption = Me.Caption
  fR.Visible = True
  Pause fR
  If fR.blnCancel Then
    Cancel = True
    Set fR = Nothing
    Exit Sub
  End If
  
  sModel = fR.comModel
  
  Set fR = Nothing

Exit Sub
errorHandler:
ErrHand Me.Name, "Report_Open"
Exit Sub
End Sub