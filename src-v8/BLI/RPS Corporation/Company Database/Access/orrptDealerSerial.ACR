Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database

Private Sub Report_Open(Cancel As Integer)
On Error GoTo errorHandler

  Dim fR As New Form_frptEnterDatesDealer
  If mbTomCat Then
    Me.Caption = "TC Orders: Dealer/Serial # Report"
    Me.lblHeading.Caption = "Tom Cat Dealer/Serial # Report"
    iType = 1
    fR.comDealerName.RowSource = "SELECT DealerName from tblAllDealers " _
      & "Where CurrentDealer = 1 and DealerType = 1 GROUP BY DealerName ORDER BY DealerName"
  Else
    Me.Caption = "FC Orders: Dealer/Serial # Report"
    Me.lblHeading.Caption = "Factory Cat Dealer/Serial # Report"
    iType = 0
    fR.comDealerName.RowSource = "SELECT DealerName from tblAllDealers " _
      & "Where CurrentDealer = 1 and DealerType = 0 GROUP BY DealerName ORDER BY DealerName"
  End If
  fR.Caption = Me.Caption
  fR.lblDates.Caption = "Shipped Date:"
  fR.Visible = True
  Pause fR
  If fR.blnCancel Then
    Cancel = True
    Set fR = Nothing
    Exit Sub
  End If

  sDealerName = fR.comDealerName
  datFromDate = fR.datFrom
  datToDate = fR.datTo

  If fR.datFrom <> "" Or fR.datTo <> "" Then
    Me.lblDates.Caption = "Shipped date: " & FormatFromTo(fR.datFrom, fR.datTo)
  End If

  If fR.comDealerName <> "" Then
    Me.DealerHeader.ForceNewPage = 0
  Else
    Me.DealerHeader.ForceNewPage = 1
  End If

  Set fR = Nothing

Exit Sub
errorHandler:
ErrHand Me.Name, "Report_Open"
Exit Sub
End Sub