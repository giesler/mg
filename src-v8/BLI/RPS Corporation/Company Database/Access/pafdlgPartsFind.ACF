Option Compare Database
Option Explicit

Public fParts As Form_pafrmParts

Private Sub cmdClose_Click()
Me.Visible = False
End Sub

Private Sub cmdFind_Click()
On Error GoTo cmdFind_Err

Dim sCrit As String, sField As String

Me.lvParts.ListItems.Clear

If Me.comField = "Vendor Name" And Me.comMatch = "Match Whole Field" Then
  If IsNull(Me.comFind) Then
    MsgBox "You must choose a vendor to find!", vbCritical
    Me.comFind.SetFocus
    Exit Sub
  End If
ElseIf IsNull(Me.txtFind) Then
  MsgBox "You must enter text to find!", vbCritical
  Me.txtFind.SetFocus
  Exit Sub
End If

If Me.comField = "Vendor Name" And Me.comMatch = "Match Whole Field" Then
  sCrit = "pv.VendorID = " & Me.comFind
  sField = "ve.VendorName"
Else
  Select Case Me.comField
    Case "Vendor Name"
      sField = "ve.VendorName"
    Case "Vendor Part Name"
      sField = "pv.VendorPartName"
    Case "Vendor Part Number"
      sField = "pv.VendorPartNum"
  End Select
  Select Case Me.comMatch
    Case "Match Whole Field"
      sCrit = sField & " = '" & Me.txtFind & "'"
    Case "Start of Field"
      sCrit = sField & " Like '" & Me.txtFind & "%'"
    Case "Any Part of Field"
      sCrit = sField & " Like '%" & Me.txtFind & "%'"
  End Select
End If

Dim rs As Recordset, sSQL As String, li As ListItem

Set li = Me.lvParts.ListItems.Add(, , "Searching...")
Me.Repaint
Me.lvParts.ListItems.Clear

sSQL = "select pa.PartID, pa.RPSPartNum, pv.VendorPrimary, " & sField & " from dbo.tblParts pa, dbo.tblPartsVendors pv, dbo.tblVendors ve "
sSQL = sSQL & "where pa.PartID = pv.PartID and ve.VendorID = pv.VendorID and " & sCrit
sSQL = sSQL & " order by pa.RPSPNSort"
Set rs = New Recordset
rs.Open sSQL, CurrentProject.Connection, adOpenForwardOnly, adLockReadOnly

If rs.BOF And rs.EOF Then Set li = Me.lvParts.ListItems.Add(, , "No records found")
  
On Error Resume Next
Do While Not rs.EOF
  Set li = Me.lvParts.ListItems("id:" & rs!PartID)
  If Err Then
    Set li = Me.lvParts.ListItems.Add(, "id:" & rs!PartID, rs!RPSPartNum, , IIf(rs!VendorPrimary, "primary", "secondary"))
    li.TooltipText = rs.Fields(Mid$(sField, InStr(sField, ".") + 1))
  End If
  rs.MoveNext
Loop
On Error GoTo cmdFind_Err
rs.Close
Set rs = Nothing

Exit Sub
cmdFind_Err:
ErrHand Me.Name, "cmdFind"
Exit Sub
End Sub

Private Sub comField_Click()

If Me.comField = "Vendor Name" And Me.comMatch = "Match Whole Field" Then
  Me.txtFind.Visible = False
  Me.comFind.Visible = True
  Me.comFind.RowSource = "select VendorID, VendorName from dbo.tblVendors order by VendorName"
  Me.comFind.ColumnCount = 2
  Me.comFind.ColumnWidths = "0;1"
Else
  Me.txtFind.Visible = True
  Me.comFind.Visible = False
End If

End Sub

Private Sub comMatch_Click()
comField_Click
End Sub

Private Sub lvParts_Click()
On Error GoTo lvParts_Err

Dim li As ListItem, rs As Recordset
Set li = Me.lvParts.SelectedItem
If li Is Nothing Then Exit Sub

Set rs = fParts.RecordsetClone
rs.MoveFirst
rs.Find "PartID = " & Val(Mid(li.Key, 4))
If rs.EOF Then
  MsgBox "The part selected, " & li.Text & " was not found.", vbExclamation
Else
  fParts.Bookmark = rs.Bookmark
End If
Set rs = Nothing
fParts.UpdateRec

Exit Sub
lvParts_Err:
ErrHand Me.Name, "lvParts"
Exit Sub
End Sub