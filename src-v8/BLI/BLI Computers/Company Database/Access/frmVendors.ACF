Private Sub Text14_DblClick(Cancel As Integer)
    Dim findme As String
    findme = InputBox$("Enter a part of a name to find.", "Find Name")
    If findme = "" Then Exit Sub
    DoCmd.GoToControl "Name"
    DoCmd.FindRecord findme, acAnywhere, False, acDown, False, acCurrent, True

End Sub

Private Sub Text22_DblClick(Cancel As Integer)
