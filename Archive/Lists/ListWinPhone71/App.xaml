<?xml version="1.0" encoding="utf-8"?>
<Application x:Class="giesler.org.lists.App" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:phone="clr-namespace:Microsoft.Phone.Controls;assembly=Microsoft.Phone" 
             xmlns:shell="clr-namespace:Microsoft.Phone.Shell;assembly=Microsoft.Phone" xmlns:clr="clr-namespace:System;assembly=mscorlib"
             xmlns:fed="clr-namespace:Microsoft.WindowsAzure.Samples.Phone.Identity.AccessControl;assembly=Phone.Identity.AccessControl">
  <!--Application Resources-->
  <Application.Resources>
    <clr:String x:Key="AppName">LISTGO</clr:String>
    <clr:String x:Key="AppNameSettingsPage">LISTGO SETTINGS</clr:String>

    <!-- ACS resources -->
    <fed:SimpleWebTokenStore x:Key="swtStore" />
    <clr:String x:Key="acsNamespace">listgo</clr:String>
        <clr:String x:Key="realm">http://listgo.mobi</clr:String>
  </Application.Resources>
  <Application.ApplicationLifetimeObjects>
    <!--Required object that handles lifetime events for the application-->
    <shell:PhoneApplicationService Launching="Application_Launching" Closing="Application_Closing" Activated="Application_Activated" Deactivated="Application_Deactivated" />
  </Application.ApplicationLifetimeObjects>
</Application>