Private Sub cmdRemove_Click()
On Error GoTo cmdRemove_Err

Dim cLI As ListItem, id As Long, iType As Integer
Set cLI = Me.lvItems.SelectedItem
id = Val(Mid(cLI.key, 3))
iType = cLI.SubItems(7)

If Me.chkPaymentReceivedFlag = True Then
  MsgBox "This invoice has already been paid.  You can't remove items from a paid invoice.", vbExclamation
  Exit Sub
End If

If cLI Is Nothing Then
  MsgBox "You can't remove items from an empty list, last I heard.", vbExclamation
  Exit Sub
End If

If MsgBox("Are you sure you want to remove the item '" & cLI.Text & "'?", vbQuestion + vbYesNo, "Confirm Delete") = vbNo Then
  Exit Sub
End If

Dim cnn As Connection, sSQL As String
Set cnn = CurrentProject.Connection

cnn.BeginTrans
cnn.Execute "DELETE From tblCustComp where afkCCompIItem = " & id
cnn.Execute "Update tblInvn SET afkInvnIItem = 0, afkInvnSItem = 0 Where afkInvnIItem = " & id
cnn.Execute "DELETE From tblInvoiceItem Where apkIItem = " & id
AssignAvailInvn False 'see if anything is now avail for sales orders
cnn.CommitTrans

Me.lvItems.ListItems.Remove cLI.Index
UpdateTotals

Exit Sub
cmdRemove_Err:
ErrHand Me.Name, "cmdRemove"
On Error Resume Next
cnn.RollbackTrans
Exit Sub

End Sub

