Private Sub cmdMoveSItemDown_Click()
On Error GoTo cmdMoveSItemDown_Err

Dim li As ListItem, pLI As ListItem, cLI As ListItem, sSQL As String, key As String

Set li = Me.lvItems.SelectedItem
key = li.key
Set pLI = Nothing
For Each cLI In Me.lvItems.ListItems
  If pLI Is Nothing Then
    Set pLI = cLI
  ElseIf pLI.key = key Then
    Exit For
  Else
    Set pLI = cLI
  End If
Next cLI

If cLI Is Nothing Then
  MsgBox "You can't move the bottom item down!", vbExclamation
  Exit Sub
End If

If pLI.key <> key Then
  MsgBox "The items key '" & li.Text & "' was not found.", vbExclamation
  Exit Sub
End If

sSQL = "UPDATE tblInvoiceItem SET IItemOrder = " & li.SubItems(1) & " WHERE apkIItem = " & Val(Mid(cLI.key, 3))
CurrentProject.Connection.Execute sSQL
sSQL = "UPDATE tblInvoiceItem SET IItemOrder = " & cLI.SubItems(1) & " WHERE apkIItem = " & Val(Mid(li.key, 3))
CurrentProject.Connection.Execute sSQL

FillItemList

For Each cLI In Me.lvItems.ListItems
  If cLI.key = key Then
    cLI.Selected = True
    Exit For
  End If
Next cLI

Exit Sub
cmdMoveSItemDown_Err:
ErrHand Me.Name, "cmdMoveSItemDown"
Exit Sub

End Sub

