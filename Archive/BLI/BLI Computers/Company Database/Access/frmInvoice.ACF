Private Sub Form_Current()
On Error GoTo Form_Current_Err

Me.lvItems.ListItems.Clear
Me.lvPayments.ListItems.Clear
Me.comafkCust = ""

If IsNull(Me.afkInvCust) Then
  If Not Me.Visible Then Me.Visible = True
  cmdSelectCust_Click
  If IsNull(Me.afkInvCust) Then
    Me.cmdVoid.Enabled = False
    Me.tabInfo.Enabled = False
    Exit Sub
  End If
  If Not Me.cmdVoid.Enabled Then Me.cmdVoid.Enabled = True
  If Not Me.tabInfo.Enabled Then Me.tabInfo.Enabled = True
End If

Me.comafkCust = DLookup("CustName", "tblCust", "apkCust = " & Me.afkInvCust)

' Check to see if 'payments' section should be enabled or disabled
If Not IsNull(Me.chkPayments) And Me.chkPayments Then
  FillPaymentList
Else
  Me.lvPayments.ListItems.Clear
End If

' Check to see if invoice is paid
If Not IsNull(Me.chkPaymentReceivedFlag) And Me.chkPaymentReceivedFlag Then
  Me.cmdAddCompSys.Enabled = False
  Me.cmdAddInvn.Enabled = False
  Me.cmdAddLabor.Enabled = False
  Me.cmdAddMiscCost.Enabled = False
  Me.cmdRemove.Enabled = False
  Me.cmdEdit.Caption = "&View"
  Me.cmdMoveSItemDown.Enabled = False
  Me.cmdMoveSItemUp.Enabled = False
  Me.chkPayments.Enabled = False
  Me.cmdVoid.Enabled = False
  Me.cmdSelectCust.Enabled = False
Else
  Me.cmdAddCompSys.Enabled = True
