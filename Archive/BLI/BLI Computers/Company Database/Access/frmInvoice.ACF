Private Sub cmdVoid_Click()
On Error GoTo cmdVoid_Err

If Me.chkPaymentReceivedFlag = True Then
    MsgBox "This invoice has already been paid.  You can't void a paid invoice.", vbExclamation
    Exit Sub
End If

If IsNull(Me.apkInv) Then
  If IsNull(Me.afkInvCust) Then
    MsgBox "You can't void an invoice with no customer information entered.", vbExclamation
    Exit Sub
  End If
End If

If Me.Dirty Then DoCmd.RunCommand acCmdSaveRecord

If MsgBox("Are you sure you want to void the current invoice?", vbQuestion + vbYesNo) = vbNo Then Exit Sub
    
Dim CurInvNum As Long, cnn As New Connection, rAf As Long, qsi As Long
cnn.Open CurrentProject.Connection.ConnectionString
qsi = Nz(Me.afkQSI, 0)
CurInvNum = Me!apkInv
OpenWaitForm "Voiding invoice..."
DoCmd.Close acForm, Me.Name
cnn.BeginTrans
cnn.Execute "UPDATE tblQSISubItems SET afkInvoiceSubItem = 0 WHERE afkInvoiceSubItem IN " _
  & "(SELECT apkIItem FROM tblInvoiceItem WHERE afkIItemInv = " & CurInvNum & ")", rAf
'cnn.Execute "UPDATE tblSale SET afkQSI = 0 WHERE afkQSI IN " _
'  & "(SELECT apkQSI FROM tblQSI WHERE afkInvoice = " & CurInvNum & ")"
cnn.Execute "UPDATE tblSaleSubItem SET afkSSItemInvn = 0 WHERE afkSSItemInvn IN " _
  & "(SELECT afkInvnIItem FROM tblInvn WHERE afkInvnIItem IN " _
  & "(SELECT apkIItem FROM tblInvoiceItem WHERE afkIItemInv = " & CurInvNum & "))"
cnn.Execute "UPDATE tblQSI SET afkInvoice = 0 WHERE afkInvoice= " & CurInvNum
cnn.Execute "DELETE From tblCustComp WHERE afkCCompIItem IN " _
  & "(SELECT apkIItem FROM tblInvoiceItem WHERE afkIItemInv = " & CurInvNum & ")", rAf
cnn.Execute "UPDATE tblInvn SET afkInvnIItem = 0, afkInvnSItem = 0 Where afkInvnIItem IN " _
  & "(SELECT apkIItem FROM tblInvoiceItem WHERE afkIItemInv = " & CurInvNum & ")", rAf
cnn.Execute "DELETE From tblInvoiceItem Where afkIItemInv = " & CurInvNum, rAf
cnn.Execute "DELETE From tblInvoicePay Where afkIPayInv = " & CurInvNum, rAf
cnn.Execute "DELETE From tblInvoice Where apkInv = " & CurInvNum, rAf
cnn.CommitTrans
cnn.Close
Set cnn = Nothing
AssignAvailInvn False 'see if anything is now avail for sales orders

If qsi <> 0 Then
  If MsgBox("The invoice you voided was converted from a sales order.  By voiding the invoice the sales order has been reactivated.  Would you like to view the sales order now?  You can void it if you wish.", vbQuestion + vbYesNo) = vbYes Then
    id = DLookup("afkSale", "tblQSI", "apkQSI = " & qsi)
    OpenSale id
  Else
    OldInvoices
  End If
Else
  OldInvoices
End If

CloseWaitForm

Exit Sub
cmdVoid_Err:
ErrHand "frmInvoice", "cmdVoid_Err"
On Error Resume Next
cnn.RollbackTrans
cnn.Close
Set cnn = Nothing
Exit Sub

End Sub

Private Sub comafkTerms_AfterUpdate()
On Error GoTo comafkTerms_AU_Err

Dim curTerms, strOldTerm, strNewTerm

curTerms = DLookup("[afkCustTerm]", "tblCust", "apkCust = " & Me.afkInvCust)

' check to see if a term is set up yet
If Not IsNull(curTerms) And curTerms <> 0 Then
  If curTerms <> Me.comafkTerms Then
    strOldTerm = DLookup("[Term]", "tblTerm", "apkTerm = " & curTerms)
    strNewTerm = DLookup("[Term]", "tblTerm", "apkTerm = " & Me.comafkTerms)
    If MsgBox("Would you like to change standard terms for this customer from '" & strOldTerm & "' to '" _
      & strNewTerm & "'?", vbQuestion + vbYesNo + vbDefaultButton2) = vbYes Then
      CurrentProject.Connection.Execute "Update tblCust SET afkCustTerm = " & Me.comafkTerms & " WHERE apkCust = " & Me.afkInvCust
    End If
  End If
ElseIf Not IsNull(Me.comafkTerms) Then
  strNewTerm = DLookup("[Term]", "tblTerm", "apkTerm = " & Me.comafkTerms)
  If MsgBox("Would you like to set the standard terms for this customer to " & strNewTerm & "?", vbQuestion + vbYesNo + vbDefaultButton2) = vbYes Then
    CurrentProject.Connection.Execute "Update tblCust SET afkCustTerm = " & Me.comafkTerms & " WHERE apkCust = " & Me.afkInvCust
  End If
End If

Exit Sub
comafkTerms_AU_Err:
ErrHand Me.Name, "comafkTerms_AU_Err"
Exit Sub

End Sub

Private Sub comSelectView_Click()
On Error GoTo comSelectView_Err

If IsNull(Me.comSelectView) Then Exit Sub

If Me.cmdShowAll.Visible Then fRecNav ("showall")

Select Case Me.comSelectView
  Case "(none)"
    Me.Filter = ""
    Me.FilterOn = False
    Me.OrderBy = ""
    Me.OrderByOn = False
  Case "All Invoices by Inv #"
    Me.Filter = ""
    Me.FilterOn = True
    Me.OrderBy = "InvNumber"
    Me.OrderByOn = True
  Case "Unpaid Invoices, by Inv #"
    Me.Filter = "InvPaymentReceivedFlag = False"
    Me.FilterOn = True
    Me.OrderBy = "InvNumber"
    Me.OrderByOn = True
  Case "Paid Invoices, by Inv #"
    Me.Filter = "InvPaymentReceivedFlag = True"
    Me.FilterOn = True
    Me.OrderBy = "InvNumber"
    Me.OrderByOn = True
  Case "Invoices for current customer"
    If IsNull(Me.afkInvCust) Then
      MsgBox "There is no customer selected.", vbExclamation
    Else
      Me.Filter = "afkInvCust = " & Me.afkInvCust
      Me.FilterOn = True
      Me.OrderBy = "InvNumber"
      Me.OrderByOn = True
    End If
  Case Else
    MsgBox "The view '" & Me.comSelectView & "' was not recognized.", vbExclamation
End Select

Exit Sub
comSelectView_Err:
ErrHand Me.Name, "comSelectView"
Exit Sub


End Sub

