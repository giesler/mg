Public Function UpdateTotals()
On Error GoTo UpdateTotals_Err

Dim PreTotal As Currency, CustCred As Currency, CredTaken As Currency, id As Long

id = Nz(Me!apkInv, -1)

'Calculate Subtotal
Me.txtSubTotal = Nz(DSum("[IItemExtPrice]", "tblInvoiceItem", "afkIItemInv = " & id), 0)
Me.txtDiscount = Me.txtSubTotal * (Me.txtDiscountRate / 100)

'Calculate Tax if applicable
If Me.chkTaxExemptFlag = True Then
    Me.txtTax = 0
Else
    Me.txtTax = (Me.txtSubTotal - Me.txtDiscount) * glLocalTaxRate
End If

'Calculate grand total
Me.txtTotal = Me.txtSubTotal + Me.txtTax + Nz(Me.txtShipping, 0) - Me.txtDiscount

'Calculate our totals
Me.txtCost = Nz(DSum("[IItemExtCost]", "tblInvoiceItem", "afkIItemInv = " & id) + Me.txtShipping, 0)
Me.txtProfit = Me.txtSubTotal - Me.txtCost - Me.txtDiscount

If Me.chkPayments Then
  Me.txtInvPaymentAmt = Nz(DSum("IPayAmt", "tblInvoicePay", "afkIPayInv = " & Me!apkInv), 0)
End If
Me.txtInvAmountDue = Me.txtTotal - Me.txtInvPaymentAmt

Exit Function
UpdateTotals_Err:
ErrHand Me.Name, "UpdateTotals_Err"
Exit Function

End Function

Public Function IItem(flgAdd As Boolean, IItemType As Long)
On Error GoTo IItem_Err

Dim id As Long, sArgs As String, cLI As ListItem

Set cLI = Me.lvItems.SelectedItem

If IsNull(Me.afkInvCust) Then
    MsgBox "You must first choose a name from the drop down list!", vbCritical
    Exit Function
End If

If Not flgAdd Then
  If cLI Is Nothing Then
    MsgBox "You can't edit items in an empty list, usually.", vbExclamation
    Exit Function
  End If
End If

DoCmd.RunCommand acCmdSaveRecord

If Me.chkPaymentReceivedFlag = True And flgAdd Then
    MsgBox "This invoice has already been paid.  You can't add items to a paid invoice.", vbExclamation
    Exit Function
End If

' if in add mode, append a record
If flgAdd Then
  Dim rc As New Recordset
  rc.Open "tblInvoiceItem", CurrentProject.Connection, adOpenDynamic, adLockOptimistic
  rc.AddNew
  rc!afkIItemInv = Me!apkInv
  If cLI Is Nothing Then
    rc!IItemOrder = 1
  Else
    rc!IItemOrder = DMax("[IItemOrder]", "tblInvoiceItem", "afkIItemInv = " & Me!apkInv) + 1
  End If
  rc!IItemType = IItemType
  If IItemType = IItemInvn Or IItemType = IItemComputer Then
    rc!IItemQty = 1
  End If
  If IItemType = IItemLabor Then
    rc!IItemPrice = Nz(DLookup("[CustLaborRate]", "tblCust", "apkCust = " & Me.afkInvCust), GetLaborRate)
  End If
  rc.Update
  rc.UpdateBatch
  id = rc!apkIItem
  Set rc = Nothing
Else
  id = Val(Mid(cLI.key, 3))
End If

' if not in add mode, set itemtype
If flgAdd = False Then IItemType = cLI.SubItems(7)

Select Case IItemType
    Case IItemInvn
        fName = "frmInvoiceInvnEdit"
    Case IItemLabor
        fName = "frmInvoiceLaborEdit"
    Case IItemComputer
        fName = "frmInvoiceInvnEdit"
    Case IItemMiscCost
        fName = "frmInvoiceMiscCostEdit"
    Case Else
        MsgBox "Editing is not set up for the type of record selected (yet) - " & IItemType, vbExclamation
        Exit Function
End Select

' open form in correct mode
If flgAdd Then
  sArgs = "ADD"
Else
  If Me.chkPaymentReceivedFlag Then
    sArgs = "VIEW"
  Else
    sArgs = "EDIT"
  End If
End If

If Me.optHideCosts = False Then
  sArgs = sArgs & ", COSTS"
End If

' Add a computer record if this is a new computer
If flgAdd And IItemType = IItemComputer Then
  sSQL = "INSERT INTO tblCustComp (afkCCompCust, afkCCompInv, afkCCompIItem) VALUES ("
  sSQL = sSQL & Me!afkInvCust & ", " & Me!apkInv & ", " & id & ")"
  CurrentProject.Connection.Execute sSQL
End If

If IItemType = IItemComputer Then
  sArgs = sArgs & ", COMPUTER"
End If

DoCmd.OpenForm fName, acNormal, , "apkIItem = " & id, acFormEdit, acDialog, sArgs

Dim fr As Form

Set fr = Forms(fName)
Dim blnCancel As Boolean
blnCancel = fr.blnCancel
DoCmd.Close acForm, fName
If Not blnCancel And Not Me.chkPaymentReceivedFlag Then
  UpdateItem id, flgAdd
  UpdateTotals
Else
  ' check if addition was cancelled
  If flgAdd Then
    If IItemType = IItemComputer Then
      sSQL = "DELETE FROM tblCustComp WHERE afkCCompIItem = " & id
      CurrentProject.Connection.Execute sSQL
    End If
    sSQL = "DELETE FROM tblInvoiceItem WHERE apkIItem = " & id
    CurrentProject.Connection.Execute sSQL
  End If
End If
        
Exit Function
IItem_Err:
ErrHand Me.Name, "IItem"
Exit Function

End Function

Public Function PrintSale(ptMode As Integer)
On Error GoTo PrintSale_Err

If IsNull(Me.afkInvCust) Then
    MsgBox "There is no sale information entered.", vbExclamation
    Exit Function
End If

If Me.chkPaymentReceivedFlag = False Then
  UpdateTotals
  DoCmd.RunCommand acCmdSaveRecord
End If

CheckMoney:
If Me.txtTotal = 0 Then
    If MsgBox("The total sale amount is 0.  Is that OK?", vbYesNo + vbQuestion) = vbNo Then
        Exit Function
    End If
End If

lngID = Me!apkInv
DoCmd.OpenReport "rptInvoice", ptMode, , "apkInv = " & lngID

If Me.chkDupPrinted Then
  If MsgBox("Would you like to print the duplicate invoice and the original invoice?" & vbLf & "The 'Duplicate Invoice' check box is checked which means you checked it or a duplicate has already been printed in batch mode.", vbQuestion + vbYesNo) = vbYes Then
      DoCmd.OpenReport "rptInvoiceDupe", ptMode, , "[apkInv] = " & Me!apkInv
      Exit Function
  End If
End If


Exit Function
PrintSale_Err:
ErrHand Me.Name, "PrintSale"
Exit Function

End Function

Public Function FillItemList()
On Error GoTo FillItemList_Err

Dim cLI As ListItem, sSQL As String, rItems As New Recordset

Me.lvItems.ListItems.Clear

If IsNull(Me!apkInv) Then Exit Function

sSQL = "SELECT apkIItem id, IItemDesc idesc, IItemOrder sort, IItemCost cost, IItemPrice price, " _
    & "IItemQty qty, IItemExtCost extcost, IItemExtPrice extprice, IItemType type " _
    & "FROM tblInvoiceItem WHERE afkIItemInv = " & Me!apkInv
rItems.Open sSQL, CurrentProject.Connection, adOpenForwardOnly, adLockReadOnly

Do While Not rItems.EOF
  UpdateItem rItems!id, True, rItems
  rItems.MoveNext
Loop

Exit Function
FillItemList_Err:
ErrHand Me.Name, "FillItemList"
Exit Function

End Function

Function UpdateItem(id As Long, blnNotInList As Boolean, Optional r As Recordset)

Dim sSQL As String, cLI As ListItem, flgCloseR As Boolean

' Open recordset if we need to
If r Is Nothing Then
  flgCloseR = True
  Set r = New Recordset
  sSQL = "SELECT apkIItem id, IItemDesc idesc, IItemOrder sort, IItemCost cost, IItemPrice price, " _
    & "IItemQty qty, IItemExtCost extcost, IItemExtPrice extprice, IItemType type " _
    & "FROM tblInvoiceItem WHERE apkIItem = " & id
  r.Open sSQL, CurrentProject.Connection, adOpenForwardOnly, adLockReadOnly
End If

' Check if we just have to add, otherwise find the right list item
If blnNotInList Then
  Set cLI = Me.lvItems.ListItems.Add(, "s:" & r!id)
Else
  For Each cLI In Me.lvItems.ListItems
    If cLI.key = "s:" & id Then Exit For
  Next cLI
End If

'Update information
cLI.Text = CutStringAtEnter(Nz(r!iDesc))
cLI.SubItems(1) = IIf(r!sort < 10, "0" & r!sort, r!sort)
cLI.SubItems(2) = Format(r!Cost, "Currency")
cLI.SubItems(3) = Format(r!price, "Currency")
cLI.SubItems(4) = r!Qty
cLI.SubItems(5) = Format(r!ExtCost, "Currency")
cLI.SubItems(6) = Format(r!ExtPrice, "Currency")
cLI.SubItems(7) = r!Type
Select Case r!Type
  Case IItemInvn
    cLI.SmallIcon = "inventory"
  Case IItemComputer
    cLI.SmallIcon = "computer"
  Case IItemLabor
    cLI.SmallIcon = "labor"
  Case IItemMiscCost
    cLI.SmallIcon = "misccost"
End Select

If flgCloseR Then
  r.Close
End If

End Function

Public Function FillPaymentList()
On Error GoTo FillPaymentList_Err

Dim cLI As ListItem, sSQL As String, rItems As New Recordset

Me.lvPayments.ListItems.Clear

If IsNull(Me!apkInv) Then Exit Function
sSQL = "SELECT apkIPay, IPayDate, IPayAmt FROM tblInvoicePay " _
  & "WHERE afkIPayInv = " & Me!apkInv
rItems.Open sSQL, CurrentProject.Connection, adOpenForwardOnly, adLockReadOnly

If Not Me.chkPaymentReceivedFlag Then
  Set cLI = Me.lvPayments.ListItems.Add(, "add", "<add>")
  cLI.SubItems(1) = " "
End If

Do While Not rItems.EOF
  Set cLI = Me.lvPayments.ListItems.Add(, "y:" & rItems!apkIPay, Format(rItems!IPayDate, "mm/dd"))
  cLI.SubItems(1) = Format(rItems!IPayAmt, "Currency")
  cLI.SubItems(2) = CLng(rItems!IPayDate)
  rItems.MoveNext
Loop

Exit Function
FillPaymentList_Err:
ErrHand Me.Name, "FillPaymentList_Err"
Exit Function

End Function


