Public Function InitInvoice()
On Error GoTo InitInvoice_Err

If IsNull(Me.afkInvCust) Then
  MsgBox "InitInvoice called with no customer selected.", vbExclamation
End If

Dim sDate As String

Me.comafkEmp = CurUserID
sDate = Right(str(year(Date)), 2) + Format(month(Date), "00") + Format(Day(Date), "00")
Me!InvNumberDate = sDate
Me!InvNumberToday = Nz(DMax("InvNumberToday", "tblInvoice", "InvNumberDate = " & sDate), 0) + 1
Me.txtNumber = sDate & "-" & Format(Me!InvNumberToday, "00")
Me.txtDate = Date
Me.chkDupPrinted = False
Me.comafkTerms = DLookup("[afkCustTerm]", "tblCust", "[apkCust] = " & Me.afkInvCust)
If Not Me.cmdVoid.Enabled Then Me.cmdVoid.Enabled = True
If Not Me.tabInfo.Enabled Then Me.tabInfo.Enabled = True
If Me.Dirty Then DoCmd.RunCommand acCmdSaveRecord

Exit Function
InitInvoice_Err:
ErrHand m_ModName, "InitInvoice"
Exit Function

