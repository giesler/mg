Private Sub Form_BeforeUpdate(Cancel As Integer)
On Error GoTo Form_BU_Err

If Me!InvPaymentReceivedFlag And Me.chkPaymentReceivedFlag.OldValue <> False Then
    MsgBox "You cannot make changes to a paid invoice.", vbExclamation
    Cancel = True
    Me.Undo
End If

If Me.txtDate = Me.txtDate.OldValue Then Exit Sub

Dim curMax As Long, mx, curDay As Long, msg As String
If Me.txtDate.OldValue <> Me.txtDate Then
  msg = "Do you want to change the invoice number on this invoice to reflect the new invoice date?"
  If MsgBox(msg, vbQuestion + vbYesNo) = vbYes Then
    curDay = Right(str(year(Me.txtDate)), 2) + Format(month(Me.txtDate), "00") + Format(Day(Me.txtDate), "00")
    Me!InvNumberDate = curDay
    mx = DMax("InvNumberToday", "tblInvoice", "InvNumberDate = " & curDay)
    If IsNull(mx) Then
        curMax = 0
    Else
        curMax = mx
    End If
    Me!InvNumberToday = curMax + 1
    Me.txtNumber = curDay & "-" & Format(Me!InvNumberToday, "00")
  End If
End If

Exit Sub
Form_BU_Err:
ErrHand m_ModName, "Form_BeforeUpdate"
Exit Sub

End Sub

