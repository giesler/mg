Private Sub lvPayments_DblClick()
On Error GoTo lvPayment_DC_Err

Dim fP As New Form_frmInvoicePaymentEdit, rP As New Recordset, id As Long
Dim li As ListItem, blnAdd As Boolean

Set li = Me.lvPayments.SelectedItem
If li Is Nothing Then Exit Sub

If li.key = "add" Then
  With rP
    .Open "tblInvoicePay", CurrentProject.Connection, adOpenDynamic, adLockOptimistic
    .AddNew
    !afkIPayInv = Me!apkInv
    !afkIPayEmp = CurUserID
    !IPayDate = Date
    .Update
    id = !apkIPay
    .Close
  End With
  blnAdd = True
Else
  id = Val(Mid(li.key, 3))
  blnAdd = False
End If

If Me.chkPaymentReceivedFlag Then fP.RecordsetType = conSnapshot
fP.Filter = "apkIPay = " & id
fP.FilterOn = True
fP.blnAdd = blnAdd
Pause fP

If blnAdd And fP.blnCancel Then
  CurrentProject.Connection.Execute "DELETE FROM tblInvoicePay WHERE apkIPay = " & id
End If

If Not fP.blnCancel Then
  FillPaymentList
  UpdateTotals
End If

Exit Sub
lvPayment_DC_Err:
ErrHand Me.Name, "lvPayment_DC_Err"
Exit Sub

End Sub

Private Sub lvPayments_KeyUp(KeyCode As Integer, ByVal Shift As Integer)
On Error GoTo lvPayments_KeyUp_Err

If KeyCode = vbKeyDelete Then
  If Me.chkPaymentReceivedFlag Then
    MsgBox "You can't delete a payment from an invoice that has been paid.", vbExclamation
    Exit Sub
  End If
  
  Dim sMsg As String, sSQL As String, li As ListItem

  Set li = Me.lvPayments.SelectedItem
  If li Is Nothing Then Exit Sub
  If li.key = "add" Then Exit Sub
  
  sMsg = "Are you sure you want to remove the payment of " & li.SubItems(1) & "?"
  If MsgBox(sMsg, vbQuestion + vbYesNo) = vbYes Then
    sSQL = "DELETE FROM tblInvoicePay WHERE apkIPay = " & Val(Mid(li.key, 3))
    CurrentProject.Connection.Execute sSQL
    FillPaymentList
    UpdateTotals
  End If

End If

Exit Sub
lvPayments_KeyUp_Err:
ErrHand Me.Name, "lvPayment_KeyUp"
Exit Sub

End Sub

