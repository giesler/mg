CodeBehindForm
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database
Option Explicit

Public blnCancel As Boolean
Public blnNew As Boolean
Public lngCustID As Long

Private Const m_ModName = "fdlgSelectCust"

Private Sub cmdCancel_Click()
On Error GoTo cmdCancel_Err

lngCustID = 0
blnCancel = True
Me.Visible = False

Exit Sub
cmdCancel_Err:
ErrHand m_ModName, "cmdCancel_Click"
Exit Sub

End Sub

Private Sub cmdNew_Click()
On Error GoTo cmdNew_Err

blnNew = True
lngCustID = 0
Me.Visible = False

Exit Sub
cmdNew_Err:
ErrHand Me.Name, "cmdNew_Click"
Exit Sub
End Sub

Private Sub cmdOK_Click()
On Error GoTo cmdOK_Err

Dim li As ListItem

Set li = Me.lvItems.SelectedItem
If li Is Nothing Then
  MsgBox "You must select a customer or click 'New' to add a new customer!", vbCritical
  Exit Sub
End If

blnCancel = False
lngCustID = Val(Mid(li.key, 3))
Me.Visible = False

Exit Sub
cmdOK_Err:
ErrHand m_ModName, "cmdOK_Click"
Exit Sub

End Sub

Private Sub Form_Load()
On Error GoTo Form_Load_Err

Dim n As Node

Set n = Me.tvCustType.Nodes.Add(, , , "Business")
Set n = Me.tvCustType.Nodes.Add(, , , "Home")
Set n = Me.tvCustType.Nodes.Add(, , , "All")

Exit Sub
Form_Load_Err:
ErrHand m_ModName, "Form_Load"
Exit Sub

End Sub

Private Sub lvItems_DblClick()
cmdOK_Click
End Sub

Private Sub tvCustType_Click()
On Error GoTo tvCustType_Err

Dim n As Node
Set n = Me.tvCustType.SelectedItem
If n Is Nothing Then Exit Sub

Dim r As New Recordset, li As ListItem, sSQL As String

sSQL = "select apkCust, CustName, CustBusinessFlag from tblCust"
Select Case n.Text
  Case "Home"
    sSQL = sSQL & " where CustBusinessFlag = 0"
  Case "Business"
    sSQL = sSQL & " where CustBusinessFlag = -1"
End Select

Me.lvItems.ListItems.Clear
r.Open sSQL, CurrentProject.Connection, adOpenForwardOnly, adLockReadOnly
Do While Not r.EOF
  Set li = Me.lvItems.ListItems.Add(, "c:" & r!apkCust, r!CustName)
  If r!CustBusinessFlag Then
    li.SmallIcon = "business"
  Else
    li.SmallIcon = "home"
  End If
  r.MoveNext
Loop
r.Close
Set r = Nothing

Exit Sub
tvCustType_Err:
ErrHand m_ModName, "tvCustType_Click"
Exit Sub

End Sub