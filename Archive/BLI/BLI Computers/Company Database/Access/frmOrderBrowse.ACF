Me.lvInvn.Sorted = True
If InStr(UCase(Me.OpenArgs), "CMDSHOWALL") Then
    Me!cmdShowAll.Visible = True
End If
wHeight = 0
End Sub

Private Sub Form_Resize()
On Error GoTo Form_Resize_Err

Dim topHeight As Long, detHeight As Long, curWindowHeight

' Check to see if window was resized
curWindowHeight = GetWindowHeight
If wHeight = curWindowHeight Then
    Exit Sub
End If

wHeight = curWindowHeight

topHeight = 1680    ' height above item subform
detHeight = 6660    ' detail height before resizing

Me.lvInvn.Height = 1000
Me.Detail0.Height = detHeight

Me.Detail0.Height = wHeight * TwipsPerPixelY - Me.FormFooter.Height - 1600
Me.lvInvn.Height = Me.Detail0.Height - topHeight - 25

Exit Sub
Form_Resize_Err:
ErrHand Me.Name, "Form_Resize"
Exit Sub

End Sub

Private Sub lvInvn_DblClick()
On Error GoTo lvInvn_DblClick_Err

Dim fr As New Form_fdlgInvnInfo, id As Long, li As ListItem, cOldCost As Currency
Set li = Me.lvInvn.SelectedItem

If li Is Nothing Then
  MsgBox "You can't edit nothing.", vbExclamation
  Exit Sub
End If
id = Val(Mid(li.key, 3))
fr.Filter = "apkInvn = " & id
fr.FilterOn = True
If Me.optHideCosts Then fr.txtInvnCost.Visible = False
fr.Visible = True
cOldCost = fr.txtInvnCost
Pause fr

If Not fr.blnCancel Then
  li.Text = fr.comafkInvnGroup.Column(1) & "\" & fr.txtInvnDesc
  li.SubItems(1) = Format(fr.txtInvnCost, "Currency")
  li.SubItems(2) = Nz(fr.comafkInvnSItem.Column(2), "")
  li.SubItems(3) = Nz(fr.comafkInvnIItem.Column(2), "")
  If fr.txtInvnCost <> cOldCost Then
    cnt = DCount("apkInvn", "tblInvn", "afkInvnOrder = " & Me!apkOrder & " AND InvnDesc = '" & fr.txtInvnDesc & "'")
    If cnt > 1 Then
      If MsgBox("Do you want to update the price on the other matching items on this order?", vbQuestion + vbYesNo) = vbYes Then
        CurrentProject.Connection.Execute "Update tblInvn SET InvnCost = " & fr.txtInvnCost & " " _
          & "WHERE afkInvnOrder = " & Me!apkOrder & " AND apkInvn <> " & fr!apkInvn & " AND InvnDesc = '" & fr.txtInvnDesc & "'"
        Set fr = Nothing
        Form_Current
      End If
    End If
  End If
End If

Exit Sub
lvInvn_DblClick_Err:
ErrHand Me.Name, "lvInvn_DblClick_Err"
Exit Sub

End Sub

Private Sub lvInvn_KeyUp(KeyCode As Integer, ByVal Shift As Integer)
If KeyCode = vbKeyDelete Then MsgBox "You can't delete inventory items from an order.", vbExclamation
End Sub

Private Sub optHideCosts_Click()
On Error GoTo optHideCosts_Err

Dim ch As ColumnHeader

If Me.optHideCosts = True Then
  Set ch = Me.lvInvn.ColumnHeaders(2)
  ch.Width = 0
Else
  If SecureAuthorize = False Then
    Me.optHideCosts = True
    Exit Sub
  End If
  Set ch = Me.lvInvn.ColumnHeaders(2)
  ch.Width = 1000
End If

Exit Sub
optHideCosts_Err:
ErrHand Me.Name, "optHideCosts"
Exit Sub

End Sub