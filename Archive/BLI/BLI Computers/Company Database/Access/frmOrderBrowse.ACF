CodeBehindForm
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database   'Use database order for string comparisons

Dim wHeight As Long

Private Sub Form_Current()
On Error GoTo Form_Current_Err

Dim sSQL As String
Dim rstInvn As New Recordset, cLI As ListItem
Dim sli As ListItem, sIndex As Long, sWhere As String

sSQL = "SELECT tblInvn.apkInvn, tblInvnGrp.InvnGrpDesc, tblInvn.InvnDesc, tblInvn.InvnCost, tblSale.SaleNumber, tblInvoice.InvNumber " _
  & "FROM tblInvoice INNER JOIN tblInvoiceItem ON tblInvoice.apkInv = tblInvoiceItem.afkIItemInv RIGHT OUTER JOIN " _
  & "    tblOrder INNER JOIN tblInvn ON tblOrder.apkOrder = tblInvn.afkInvnOrder INNER JOIN " _
  & "    tblInvnGrp ON tblInvn.afkInvnGroup = tblInvnGrp.apkInvnGrp ON tblInvoiceItem.apkIItem = tblInvn.afkInvnIItem LEFT OUTER JOIN " _
  & "    tblSaleItem INNER JOIN tblSale ON tblSaleItem.afkSItemSale = tblSale.apkSale ON " _
  & "    tblInvn.afkInvnSItem = tblSaleItem.apkSItem "
sSQL = sSQL & "WHERE (tblOrder.apkOrder = " & Me!apkOrder & ")"

rstInvn.Open sSQL, CurrentProject.Connection, adOpenForwardOnly, adLockReadOnly

Me.lvInvn.ListItems.Clear

Do While Not rstInvn.EOF
  Set cLI = Me.lvInvn.ListItems.Add(, "n:" & rstInvn!apkInvn, rstInvn!InvnGrpDesc & "\" & rstInvn!InvnDesc)
  cLI.SubItems(1) = Format(rstInvn!InvnCost, "Currency")
  cLI.SubItems(2) = Nz(rstInvn!SaleNumber, "")
  cLI.SubItems(3) = Nz(rstInvn!InvNumber, "")
  rstInvn.MoveNext
Loop

Exit Sub
Form_Current_Err:
ErrHand Me.Name, "Form_Current_Err"
Exit Sub

End Sub

Private Sub Form_Deactivate()
DoCmd.Close acForm, "fdlgFind"
End Sub

