Option Compare Database   'Use database order for string comparisons

Private Sub Button20_Click()
    DoCmd.OpenForm "frm_prices_print selection"
End Sub

Private Sub Button21_Click()
    DoCmd.Close
End Sub

Private Sub chkNet10_Click()

If Me![Business] = False Then
    Me![chkNet10] = False
    MsgBox "The Net 10 discount is only for business customers.", 16, "BLI Computers"
    Exit Sub
End If

If Me![chkNet10] = True Then
    If Me![Total Sale] < glMinSaleAmt Then
        Me![chkNet10] = False
        MsgBox "The Net 10 discount is only for sales greater then $" & glMinSaleAmt & ".", 16, "BLI Computers"
        Exit Sub
    End If
    Me![Discount Applied] = Me![Total Sale] * glDefDiscountRate
ElseIf Me![chkNet10] = False Then
    Me![Discount Applied] = 0
End If

End Sub

Private Sub cmdPrint_Click()
On Error GoTo cmdPrint_Err
DoCmd.OpenReport "rptAcntReceive", acViewNormal

Exit Sub
cmdPrint_Err:
ErrHand Me.Name, "cmdPrint"
Exit Sub
End Sub

Private Sub cmdPrintPreview_Click()
On Error GoTo cmdPrintPreview_Err
DoCmd.OpenReport "rptAcntReceive", acViewPreview

Exit Sub
cmdPrintPreview_Err:
ErrHand Me.Name, "cmdPrintPreview"
Exit Sub

End Sub

Private Sub cmdRefresh_Click()
Me![subUnpaidSales].Requery
End Sub


Private Sub optHideCosts_Click()

If Me.optHideCosts = True Then
    Me.lblCost.Visible = False
    Me.lblProfit.Visible = False
    Me![subUnpaidSales].Form![Total Cost].Visible = False
    Me![subUnpaidSales].Form![Profit].Visible = False
    Me![subUnpaidSales].Form![tTotalCost].Visible = False
    Me![subUnpaidSales].Form![tTotalProfit].Visible = False
    Me![subUnpaidSales].Form![tTotalSale].Visible = False
Else
    If SecureAuthorize = False Then
        Me.optHideCosts = True
        Exit Sub
    End If
    Me.lblCost.Visible = True
    Me.lblProfit.Visible = True
    Me![subUnpaidSales].Form![Total Cost].Visible = True
    Me![subUnpaidSales].Form![Profit].Visible = True
    Me![subUnpaidSales].Form![tTotalCost].Visible = True
    Me![subUnpaidSales].Form![tTotalProfit].Visible = True
    Me![subUnpaidSales].Form![tTotalSale].Visible = True
End If

End Sub