Private Sub Report_NoData(Cancel As Integer)

  MsgBox "There is no matching catalog data.", vbExclamation, Me.Caption
  Cancel = True
  
End Sub

Private Sub Report_Open(Cancel As Integer)
On Error GoTo errHandler

  Dim fC As Form_frptCust, strCrit As String
  Set fC = New Form_frptCust
  fC.Caption = Me.Caption
  
  Pause fC

  If fC.mblnCancel Then
    Cancel = True
  Else
    SetSPVar "CustID", fC.cmbSoldName.Column(1)
    If fC.cmbSoldName <> "" Then
      strCrit = "Customer: " + fC.cmbSoldName
      Me.lblCriteria.Caption = strCrit
    End If
  End If
  
  Set fC = Nothing

Exit Sub
errHandler:
ErrHand Me.Name, "Report_Open"
Exit Sub
End Sub