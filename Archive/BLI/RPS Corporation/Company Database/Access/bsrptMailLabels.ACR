Private Sub Report_Open(Cancel As Integer)

Dim fR As New Form_isfrptStateCode, sFilt As String
fR.Caption = "BSCAI List"
fR.Visible = True
Pause fR
If fR.blnCancel Then
  Cancel = True
  Exit Sub
End If

If fR.txtState <> "" Then
  sFilt = "State = '" & fR.txtState & "'"
End If

If fR.txtCode <> "" Then
  If sFilt <> "" Then sFilt = sFilt & " AND "
  sFilt = sFilt & "Code = '" & fR.txtCode & "'"
End If

Me.ServerFilter = sFilt
Set fR = Nothing

End Sub