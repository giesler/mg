Option Compare Database
Option Explicit

Private Sub Report_Open(Cancel As Integer)

Dim fR As New Form_frptEnterModel
fR.Caption = "Option Parts by Model"
fR.Visible = True
Pause fR
If fR.blnCancel Then
    Cancel = True
    Set fR = Nothing
    Exit Sub
End If

Dim strFilter As String

If fR.comModel <> "" Then
    strFilter = "[Model] = '" & fR.comModel & "'"
End If

If strFilter <> "" Then
    Me.ServerFilter = strFilter
    Me.FilterOn = True
    Me.ModelFooter.ForceNewPage = 0     'only one model so don't force break at end
Else
  Me.ServerFilter = ""
  Me.FilterOn = False
End If

Set fR = Nothing

End Sub