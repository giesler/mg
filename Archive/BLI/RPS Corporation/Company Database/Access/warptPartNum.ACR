Option Compare Database   'Use database order for string comparisons

Private Sub Report_NoData(Cancel As Integer)

  MsgBox "There were no warranty claims for the criteria entered.", vbExclamation
  Cancel = True

End Sub

Private Sub Report_Open(Cancel As Integer)

  Dim fR As New Form_wafrptWarrantyCost
  fR.chkPageBreak.Visible = False
  If mbTomCat Then
    iType = 1
    Me.Caption = "TC Warranty: Report by Part Number"
    Me.lblHead.Caption = "Tom Cat Warranty by Part Number"
    fR.cmbDealerName.RowSource = "SELECT DealerName FROM dbo.tblAllDealers WHERE (CurrentDealer = 1) AND (DealerType = 1) GROUP BY DealerName ORDER BY DealerName"
  Else
    iType = 0
    Me.Caption = "FC Warranty: Report by Part Number"
    Me.lblHead.Caption = "Factory Cat Warranty by Part Number"
    fR.cmbDealerName.RowSource = "SELECT DealerName FROM dbo.tblAllDealers WHERE (CurrentDealer = 1) AND (DealerType = 0) GROUP BY DealerName ORDER BY DealerName"
  End If
  
  fR.Caption = Me.Caption
  fR.lstOpenWarranty.Selected(2) = True
  fR.Visible = True
  Pause fR
  If fR.blnCancel Then
    Cancel = True
    Set fR = Nothing
    Exit Sub
  End If

  SetSPVar "DealerName", fR.cmbDealerName
  SetSPVar "PartID", fR.cmbPartID
  SetSPVar "Open", fR.lstOpenWarranty
  SetSPVar "FailFromDate", fR.txtFailFrom
  SetSPVar "FailToDate", fR.txtFailTo
  SetSPVar "EnterFromDate", fR.txtEnterFrom
  SetSPVar "EnterToDate", fR.txtEnterTo
  SetSPVar "CMFromDate", fR.txtCMFrom
  SetSPVar "CMToDate", fR.txtCMTo
  
  Dim strMsg As String
  
  If fR.txtFailFrom <> "" Or fR.txtFailTo <> "" Then
    strMsg = "Date of Failure: " & FormatFromTo(fR.txtFailFrom, fR.txtFailTo)
  End If
  
  If fR.txtEnterFrom <> "" Or fR.txtEnterTo <> "" Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & "Date entered: " & FormatFromTo(fR.txtEnterFrom, fR.txtEnterTo)
  End If
  
  If fR.txtCMFrom <> "" Or fR.txtCMTo <> "" Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & "CM Date: " & FormatFromTo(fR.txtCMFrom, fR.txtCMTo)
  End If
  
  If fR.cmbDealerName <> "" Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & "Dealer: " & fR.cmbDealerName
  End If
  
  If fR.cmbPartID <> "" Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & "Part: " & fR.cmbPartID.Column(1)
  End If
  
  If Not IsNull(fR.lstOpenWarranty) Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & fR.lstOpenWarranty.Column(1)
  End If
  
  If strMsg <> "" Then
    Me.lblHeading.Caption = strMsg
  End If
  
  Set fR = Nothing
  
  
End Sub