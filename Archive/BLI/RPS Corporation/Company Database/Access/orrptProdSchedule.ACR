Option Compare Database

Private Sub Report_Open(Cancel As Integer)
On Error GoTo errorHandler

  If mbTomCat Then
    Me.Caption = "TC Orders: Production Schedule"
    Me.lblHeading.Caption = "Tom Cat Production Schedule"
    iType = 1
  Else
    Me.Caption = "FC Orders: Production Schedule"
    Me.lblHeading.Caption = "Factory Cat Production Schedule"
    iType = 0
  End If

  Dim varTmp As Variant
  varTmp = InputBox("Enter the 'Week Of' title to print at the top of the report.", Me.Caption)
  If Not IsNull(varTmp) Then
    Me.lblWeekOf.Caption = "Week Of " & varTmp
  End If

Exit Sub
errorHandler:
ErrHand Me.Name, "Report_Open"
Exit Sub
End Sub