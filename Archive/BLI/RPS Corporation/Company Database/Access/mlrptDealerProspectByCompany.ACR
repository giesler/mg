Private Sub Report_Open(Cancel As Integer)

Dim fR As New Form_frptEnterState
fR.Caption = "Dealer Prospect By Company"
fR.Visible = True
Pause fR
If fR.blnCancel Then
  Cancel = True
  Exit Sub
End If

sState = fR.txtState
sCounty = fR.comCounty

If fR.txtState <> "" Then
  Me.lblCrit.Caption = "State: " & fR.txtState
  If fR.comCounty <> "" Then
    Me.lblCrit.Caption = Me.lblCrit.Caption & ", County: " & fR.comCounty
  End If
End If

End Sub