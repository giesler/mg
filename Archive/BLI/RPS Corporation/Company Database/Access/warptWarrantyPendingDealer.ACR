CodeBehindForm
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database

Private Sub Report_Open(Cancel As Integer)

If mbTomCat Then
  iType = 1
  Me.Caption = "TC Warranty: Pending by Dealer"
  Me.lblHead.Caption = "Tom Cat Warranty Pending Report"
Else
  iType = 0
  Me.Caption = "FC Warranty: Pending by Dealer"
  Me.lblHead.Caption = "Factory Cat Warranty Pending Report"
End If

Dim fR As New Form_frptEnterDatesDealerWarranty, strMsg As String
fR.Caption = Me.Caption
fR.lblDateCaption.Caption = "Date of failure"
fR.lstOpenWarranty.Selected(2) = True
fR.chkPageBreak.Visible = True
fR.Visible = True
Pause fR
If fR.blnCancel Then
  Cancel = True
  Set fR = Nothing
  Exit Sub
End If

datFromDate = fR.datFrom
datToDate = fR.datTo
sDealerName = fR.comDealer
iOpenFlag = fR.lstOpenWarranty
If fR.chkPageBreak Then
  Me.DealerFooter.ForceNewPage = 2
End If

If fR.datFrom <> "" Or fR.datTo <> "" Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & FormatFromTo(fR.datFrom, fR.datTo)
End If

If IsNull(fR.lstOpenWarranty) Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & "Open and closed claims"
ElseIf fR.lstOpenWarranty = 0 Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & "Closed claims"
ElseIf fR.lstOpenWarranty = 1 Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & "Open claims"
End If

If strMsg <> "" Then
    Me.lblHeading.Caption = strMsg
End If

Set fR = Nothing

End Sub