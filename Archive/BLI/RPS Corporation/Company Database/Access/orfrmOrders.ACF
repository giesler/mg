Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database   'Use database order for string comparisons
Option Explicit

Dim vUpdate As Integer

Private Const bTCMode = False

Private Sub btnCosts_Click()
On Error GoTo errHandler

  If Me.btnCosts = True Then
    If SecureAuthorize() = False Then
      Me.btnCosts = False
    Else
      Me.txtCostPrice.Visible = True
      Me.txtMargin.Visible = True
    End If
  Else
    Me.txtCostPrice.Visible = False
    Me.txtMargin.Visible = False
  End If

Exit Sub
errHandler:
ErrHand Me.Name, "btnCosts"
Exit Sub
End Sub

Private Sub chkMajorAccount_Click()
On Error GoTo errHandler

  If Me.chkMajorAccount Then
    Me.comMajorAccountID.Enabled = True
  Else
    Me!MajorAccountID = Null
    Me.comMajorAccountID.Enabled = False
  End If

Exit Sub
errHandler:
ErrHand Me.Name, "chkMajorAccount"
Exit Sub
End Sub

Private Sub comDealer_AfterUpdate()
On Error GoTo errHandler

  Me!fkDealerID = Me.comDealer.Column(1)
  
Exit Sub
errHandler:
ErrHand Me.Name, "comDealer_AfterUpdate"
Exit Sub
End Sub

Private Sub comMajorAccountID_Exit(Cancel As Integer)
On Error Resume Next

  Me.txtName.SetFocus
  
End Sub

Private Sub comModel_Exit(Cancel As Integer)
On Error Resume Next

  Me.txtPurchaseOrder.SetFocus
  
End Sub

Private Sub Form_Activate()
On Error GoTo errHandler

  Dim newMenu As CommandBarPopup
  Set newMenu = CBAddMenu(Me.Toolbar, "Print")
  newMenu.BeginGroup = True
  newMenu.Tag = "custom"
  Call CBAddMenuControl(newMenu, "Current record", "=PrintCurRecord()")
  Dim cbrCur As CommandBar, ctlCBarControl As CommandBarControl
  Set cbrCur = Application.CommandBars(Me.Toolbar)
  With cbrCur
    Set ctlCBarControl = cbrCur.Controls.Add(msoControlButton)
    With ctlCBarControl
      .Caption = "Copy &Order"
      .Style = msoButtonCaption
      .Enabled = True
      .OnAction = "=CopyCurRecord()"
      .BeginGroup = True
      .Tag = "custom"
    End With
  End With

  Set cbrCur = Application.CommandBars(Me.Toolbar)
  With cbrCur
    Set ctlCBarControl = cbrCur.Controls.Add(msoControlEdit)
    With ctlCBarControl
      .Caption = "Find Or&der #:"
      .Enabled = True
      .Style = msoComboLabel
      .OnAction = "=FindOrder()"
      .BeginGroup = True
      .Tag = "custom"
    End With
  End With

Exit Sub
errHandler:
ErrHand Me.Name, "Form_Activate"
Exit Sub
End Sub

Private Sub Form_ApplyFilter(Cancel As Integer, ApplyType As Integer)
On Error GoTo errHandler

  Me.Toolbar = "FCFormToolBar"
  Dim blnCancel As Boolean
  Me.ServerFilter = ChangeStars(Me.ServerFilter, blnCancel)
  If blnCancel Then Cancel = True
  Me.txtCostPrice.Enabled = True

Exit Sub
errHandler:
ErrHand Me.Name, "Form_ApplyFilter"
Exit Sub
End Sub

Private Sub Form_BeforeInsert(Cancel As Integer)
On Error GoTo errHandler

  Dim maxnum
  If IsNull(Me!OrderID) Then
    maxnum = DMax("OrderKey", "tblAllOrders", IIf(bTCMode, "OrderType=1", "OrderType=0"))
    Me!OrderKey = maxnum + 1
    If IsNull(Me.txtOrderNumber) Then
      Me.txtOrderNumber = maxnum + 1
    End If
    Me!OrderType = IIf(bTCMode, 1, 0)
  End If

Exit Sub
errHandler:
ErrHand Me.Name, "Form_BeforeInsert"
Exit Sub
End Sub

Private Sub Form_BeforeUpdate(Cancel As Integer)
On Error GoTo errHandler

  If MsgBox("Are you sure you want to save changes for this record?", vbQuestion + vbYesNo) = vbNo Then
    On Error Resume Next
    MsgBox "To restore this record to the original, press Escape after clicking OK.", vbInformation
    DoCmd.CancelEvent
  End If

Exit Sub
errHandler:
ErrHand Me.Name, "Form_BeforeUpdate"
Exit Sub
End Sub

Private Sub Form_Current()
On Error Resume Next

  If Not IsNull(Me!MajorAccount) Or Me!MajorAccount Then
    Me.comMajorAccountID.Enabled = True
  Else
    Me.comMajorAccountID.Enabled = False
  End If
  
  If Me.btnCosts Then
    Me.txtCostPrice.Visible = True
  Else
    Me.txtCostPrice.Visible = False
  End If

End Sub

Private Sub Form_Deactivate()
On Error GoTo errHandler
  
  Dim ctl As CommandBarControl
  For Each ctl In Application.CommandBars(Me.Toolbar).Controls
    If ctl.Tag = "custom" Then ctl.Delete
  Next ctl

Exit Sub
errHandler:
ErrHand Me.Name, "Form_Deactivate"
Exit Sub
End Sub

Private Sub Form_Filter(Cancel As Integer, FilterType As Integer)
On Error GoTo errHandler

  Me.Toolbar = "FCFormServerFilter"
  If Me.txtCostPrice.Visible = False Then
    Me.txtCostPrice.Enabled = False
    Me.txtMargin.Visible = False
  End If

Exit Sub
errHandler:
ErrHand Me.Name, "Form_Filter"
Exit Sub
End Sub

Public Function PrintCurRecord()
On Error GoTo errHandler

  DoCmd.RunCommand acCmdSaveRecord
  If Me.Dirty Then MsgBox "Since you did not save changes, the report will not reflect any changes you made.", vbInformation

  If flgDebug Then
    DoCmd.OpenReport "orrfrmOrders", acViewPreview, , "OrderID = " & Me!OrderID
  Else
    DoCmd.OpenReport "orrfrmOrders", acViewNormal, , "OrderID = " & Me!OrderID
  End If

Exit Function
errHandler:
ErrHand Me.Name, "PrintCurRecord"
Exit Function
End Function

Public Function CopyCurRecord()
On Error GoTo errHandler

  If MsgBox("Are you sure you want to copy this order?", 36) = 6 Then
    DoCmd.RunCommand acCmdSelectRecord
    DoCmd.RunCommand acCmdCopy
    DoCmd.RunCommand acCmdPasteAppend
  End If

Exit Function
errHandler:
ErrHand Me.Name, "CopyCurRecord"
Exit Function
End Function

Private Sub Form_Load()
On Error GoTo errHandler

  Me.ServerFilter = ""

  If bTCMode Then
    Me.Caption = "Tom Cat Orders"
    Me.RecordSource = "SELECT * FROM dbo.tblAllOrders WHERE OrderType = 1"
    Me.comModel.RowSource = "SELECT Model, Description FROM dbo.tblModels WHERE (ModelType =1) ORDER BY Model"
    Me.comDealer.RowSource = "SELECT DealerName, MIN(DealerID) FROM dbo.tblAllDealers WHERE (CurrentDealer = 1) AND (DealerType = 1) GROUP BY DealerName, DealerID ORDER BY DealerName"
    Me.comMajorAccountID.RowSource = "SELECT MajorAccountID, MACompName FROM dbo.tblAllMajorAcnts WHERE (MAType = 1) ORDER BY MACompName"
    Me.comafkServicingDealer.RowSource = _
      "SELECT MIN(DealerID) AS Expr1, DealerName FROM dbo.tblAllDealers WHERE (CurrentDealer = 1)  GROUP BY DealerName ORDER BY DealerName"
  Else
    Me.Caption = "Factory Cat Orders"
    Me.RecordSource = "SELECT * FROM dbo.tblAllOrders WHERE OrderType = 0"
    Me.comModel.RowSource = "SELECT Model, Description FROM dbo.tblModels WHERE (ModelType =0) ORDER BY Model"
    Me.comDealer.RowSource = "SELECT DealerName, MIN(DealerID) FROM dbo.tblAllDealers WHERE (CurrentDealer = 1) AND (DealerType = 0) GROUP BY DealerName, DealerID ORDER BY DealerName"
    Me.comMajorAccountID.RowSource = "SELECT MajorAccountID, MACompName FROM dbo.tblAllMajorAcnts WHERE (MAType = 0) ORDER BY MACompName"
    Me.comafkServicingDealer.RowSource = _
      "SELECT MIN(DealerID) AS Expr1, DealerName FROM dbo.tblAllDealers WHERE (CurrentDealer = 1)  GROUP BY DealerName ORDER BY DealerName"
  End If

Exit Sub
errHandler:
ErrHand Me.Name, "Form_Load"
Exit Sub
End Sub

