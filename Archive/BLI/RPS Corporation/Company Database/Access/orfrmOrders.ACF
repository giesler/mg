  Me.TimerInterval = 0

End Sub

Private Sub txtOrderNumber_AfterUpdate()

  Me!OrderKey = Val(Me.txtOrderNumber)

End Sub

Public Function FindOrder()
On Error GoTo errHandler

  Dim sOrder As String
  
  sOrder = Application.CommandBars(Me.Toolbar).Controls("Find Or&der #:").Text
  If IsNull(sOrder) Or sOrder = "" Then Exit Function
  
  Dim rst As Recordset
  
  Set rst = Me.RecordsetClone
  
  rst.Find "[OrderNumber] = '" & sOrder & "'"
  If rst.EOF Then
    MsgBox "The order number '" & sOrder & "' was not found in the database.", vbExclamation
  Else
    Me.Form.Bookmark = rst.Bookmark
    Me.txtOrderNumber.SetFocus
  End If
  rst.Close
  Set rst = Nothing

Exit Function
errHandler:
ErrHand Me.Name, "FindOrder"
Exit Function
End Function