CodeBehindForm
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database   'Use database order for string comparisons

Private Sub Report_Open(Cancel As Integer)
On Error GoTo errorHandler

  Dim fR As New Form_frptEnterDatesDealer
  fR.chkPageBreak.Visible = True
  If mbTomCat Then
    Me.Caption = "TC Part Sales Report"
    Me.txtHeading.Caption = "Tom Cat Part Sales Report"
    iType = 1
    fR.comDealerName.RowSource = "SELECT DealerName from tblAllDealers " _
      & "Where CurrentDealer = 1 and DealerType = 1 GROUP BY DealerName ORDER BY DealerName"
  Else
    Me.Caption = "FC Part Sales Report"
    Me.txtHeading.Caption = "Factory Cat Part Sales Report"
    iType = 0
    fR.comDealerName.RowSource = "SELECT DealerName from tblAllDealers " _
      & "Where CurrentDealer = 1 and DealerType = 0 GROUP BY DealerName ORDER BY DealerName"
  End If
  
  fR.Caption = Me.Caption
  fR.lblDates.Caption = "Sale Date:"
  fR.Visible = True
  Pause fR
  If fR.blnCancel Then
    Cancel = True
    Set fR = Nothing
    Exit Sub
  End If

  SetSPVar "DealerName", fR.comDealerName
  SetSPVar "FromDate", fR.datFrom
  SetSPVar "ToDate", fR.datTo
  
  If fR.datFrom <> "" Or fR.datTo <> "" Then
    Me.lblDates.Caption = FormatFromTo(fR.datFrom, fR.datTo)
  End If

  If fR.chkPageBreak Then
    Me.DealerHeader.ForceNewPage = 1
  Else
    Me.DealerHeader.ForceNewPage = 0
  End If

  Set fR = Nothing

Exit Sub
errorHandler:
ErrHand Me.Name, "Report_Open"
Exit Sub
End Sub