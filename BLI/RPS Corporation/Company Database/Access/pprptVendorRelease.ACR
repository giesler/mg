Private Sub Report_Open(Cancel As Integer)

Dim fR As New Form_ppfrptVendorRelease
Dim strCrit As String, strMsg As String

fR.Visible = True
Pause fR
If fR.blnCancel Then
    Cancel = True
    Set fR = Nothing
    Exit Sub
End If

sPOID = fR.comPO
sPartName = fR.comRPSPN
sVendorPartName = fR.comVendorPN
iOpenFlag = fR.chkIncludeOnlyOpen

If fR.comPO <> "" Then
    strMsg = "PO " & fR.comPO
End If

If fR.comRPSPN <> "" Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & "RPS P/N " & fR.comRPSPN
End If

If fR.comVendorPN <> "" Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & "Vendor P/N " & fR.comVendorPN
End If
    
If fR.chkIncludeOnlyOpen = True Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & "Open Orders"
End If

If strMsg <> "" Then
    Me.lblHeading.Caption = Me.lblHeading.Caption & " - " & strMsg
End If

Set fR = Nothing

End Sub