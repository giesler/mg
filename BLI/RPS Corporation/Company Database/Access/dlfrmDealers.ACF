Private Sub cmdDeleteAdvertAlloc_Click()
On Error GoTo errHandler

  Dim li As ListItem
  Set li = Me.lvAdvert.SelectedItem
  If li Is Nothing Then
    MsgBox "You must select an item to delete!", vbExclamation
    Exit Sub
  End If
  
  If MsgBox("Are you sure you want to delete the advertising allocation dated '" & li.Text & "'?", vbQuestion + vbYesNo) = vbYes Then
    gConn.Execute "delete from dbo.tblDealerAdvert where AdvertID = " & li.SubItems(3)
    UpdateAdvert
  End If

Exit Sub
errHandler:
ErrHand Me.Name, "cmdDeleteAdvertAlloc"
Exit Sub
End Sub

Private Sub cmdEditAdvertAlloc_Click()

  AdvertEdit False
  
End Sub

