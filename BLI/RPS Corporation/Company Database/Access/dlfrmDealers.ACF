  GoalEdit True
  
End Sub

Private Sub cmdGoalDelete_Click()
On Error GoTo errHandler

  Dim li As ListItem
  Set li = Me.lvGoals.SelectedItem
  If li Is Nothing Then
    MsgBox "You must select an item to delete!", vbExclamation
    Exit Sub
  End If
  
  If MsgBox("Are you sure you want to delete the goal for the year '" & li.Text & "'?", vbQuestion + vbYesNo) = vbYes Then
    gConn.Execute "delete from dbo.tblDealerGoals where GoalID = " & li.SubItems(5)
    UpdateGoals
  End If

Exit Sub
errHandler:
ErrHand Me.Name, "cmdGoalDelete"
Exit Sub
End Sub

