Private Sub tabDealer_Change()

  Select Case Me.tabDealer.Value
    Case iDealerGoalsPage
      UpdateGoals
    Case iAdvertAllocPage
      UpdateAdvert
  End Select

End Sub

Private Function UpdateGoals()
On Error GoTo errHandler

  Dim rs As Recordset, li As ListItem
  
  Me.lvGoals.ListItems.Clear
  
  Set rs = New ADODB.Recordset
  Set rs = gConn.Execute("dlspfrmDealerGoals " + CStr(Nz(Me!DealerID, "null")))
  
  Do While Not rs.EOF
    Set li = Me.lvGoals.ListItems.Add(, "g:" & rs!GoalID, Nz(rs!GoalYear, 0))
    li.SubItems(1) = Nz(rs!Model, "n/a")
    li.SubItems(2) = Nz(rs!Description, "n/a")
    li.SubItems(3) = Nz(rs!Goal, 0)
    li.SubItems(4) = Nz(rs!ModelID, 0)
    li.SubItems(5) = Nz(rs!GoalID, 0)
    rs.MoveNext
  Loop
  rs.Close
  Set rs = Nothing
  
Exit Function
errHandler:
ErrHand Me.Name, "UpdateGoals"
Exit Function
End Function

Private Function GoalEdit(pblnAdd As Boolean)
On Error GoTo errHandler

  If IsNull(Me.txtDealerName) Then
    MsgBox "You must enter a dealer name!", vbExclamation
    Exit Function
  End If
  
  If IsNull(Me!DealerID) Then DoCmd.RunCommand acCmdSaveRecord

  Dim f As Form_dlfdlgDealerGoals, li As ListItem, strSQL As String
  Set f = New Form_dlfdlgDealerGoals
  f.Caption = Me.txtDealerName
  
  If Not pblnAdd Then
    Set li = Me.lvGoals.SelectedItem
    If li Is Nothing Then
      MsgBox "You must select an entry to edit.", vbExclamation
      Exit Function
    End If
    f.txtYear = li.Text
    f.cmbModelID = li.SubItems(4)
    f.txtGoal = li.SubItems(3)
    f.txtYear.Enabled = False
    f.cmbModelID.Enabled = False
    f.mlngDealerID = Me!DealerID
  Else
    f.txtYear = mstrGoalYear
    f.mlngDealerID = Me!DealerID
    f.mblnVerifyUnique = True
    f.Visible = True
    f.cmbModelID.SetFocus
  End If
  
  Pause f
  
  If Not f.mblnCancel Then
    If pblnAdd Then
      strSQL = "insert tblDealerGoals (DealerID, GoalYear, ModelID, Goal) "
      strSQL = strSQL + "values (" + CStr(Me!DealerID) + ", " + CStr(f.txtYear) + ", "
      strSQL = strSQL + CStr(f.cmbModelID) + ", " + CStr(f.txtGoal) + ")"
      gConn.Execute strSQL
    Else
      strSQL = "update tblDealerGoals set "
      strSQL = strSQL + "GoalYear = " + CStr(f.txtYear) + ", "
      strSQL = strSQL + "ModelID = " + CStr(f.cmbModelID) + ", "
      strSQL = strSQL + "Goal = " + CStr(f.txtGoal) + "  "
      strSQL = strSQL + "where GoalID = " + CStr(li.SubItems(5))
      gConn.Execute strSQL
    End If
    UpdateGoals
    mstrGoalYear = f.txtYear
  End If
  
  Set f = Nothing

Exit Function
errHandler:
ErrHand Me.Name, "GoalEdit"
Exit Function
End Function


Private Function UpdateAdvert()
On Error GoTo errHandler

  Dim rs As Recordset, li As ListItem
  
  Me.lvAdvert.ListItems.Clear
  
  Set rs = New ADODB.Recordset
  Set rs = gConn.Execute("dlspfrmDealerAdvert " + CStr(Nz(Me!DealerID, "null")))
  
  Do While Not rs.EOF
    Set li = Me.lvAdvert.ListItems.Add(, "a:" & rs!AdvertID, Nz(rs!AdvertDate, ""))
    li.SubItems(1) = Format(Nz(rs!Amount, "0"), "Currency")
    li.SubItems(2) = Nz(rs!Note, "")
    li.SubItems(3) = rs!AdvertID
    rs.MoveNext
  Loop
  rs.Close
  Set rs = Nothing
  
Exit Function
errHandler:
ErrHand Me.Name, "UpdateAdvert"
Exit Function
End Function

Private Function AdvertEdit(pblnAdd As Boolean)
On Error GoTo errHandler

  If IsNull(Me.txtDealerName) Then
    MsgBox "You must enter a dealer name!", vbExclamation
    Exit Function
  End If
  
  If IsNull(Me!DealerID) Then DoCmd.RunCommand acCmdSaveRecord

  Dim f As Form_dlfdlgDealerAdvert, li As ListItem, strSQL As String
  Set f = New Form_dlfdlgDealerAdvert
  f.Caption = Me.txtDealerName
  
  If Not pblnAdd Then
    Set li = Me.lvAdvert.SelectedItem
    If li Is Nothing Then
      MsgBox "You must select an entry to edit.", vbExclamation
      Exit Function
    End If
    f.txtAdvertDate = li.Text
    f.txtAmount = li.SubItems(1)
    f.txtNote = li.SubItems(2)
  End If
  
  Pause f
  
  If Not f.mblnCancel Then
    If pblnAdd Then
      strSQL = "insert dbo.tblDealerAdvert (DealerID, AdvertDate, Amount, Note) "
      strSQL = strSQL + "values (" + CStr(Me!DealerID) + ", '" + CStr(f.txtAdvertDate) + "', "
      strSQL = strSQL + CStr(f.txtAmount) + ", '" + DQ(CStr(Nz(f.txtNote, ""))) + "')"
      gConn.Execute strSQL
    Else
      strSQL = "update dbo.tblDealerAdvert set "
      strSQL = strSQL + "AdvertDate = '" + CStr(f.txtAdvertDate) + "', "
      strSQL = strSQL + "Amount= " + CStr(f.txtAmount) + ", "
      strSQL = strSQL + "Note= '" + DQ(CStr(Nz(f.txtNote, ""))) + "'  "
      strSQL = strSQL + "where AdvertID = " + CStr(li.SubItems(3))
      gConn.Execute strSQL
    End If
    UpdateAdvert
  End If
  
  Set f = Nothing

Exit Function
errHandler:
ErrHand Me.Name, "AdvertEdit"
Exit Function
End Function

Public Function FindDealer()
On Error GoTo errHandler

  Dim sOrder As String
  
  sOrder = Application.CommandBars(Me.Toolbar).Controls("Find &Dealer:").Text
  If IsNull(sOrder) Or sOrder = "" Then Exit Function
  
  Dim rst As Recordset
  
  Set rst = Me.RecordsetClone
  
  rst.Find "[DealerName] Like '" & sOrder & "%'"
  If rst.EOF Then
    MsgBox "The dealer name '" & sOrder & "' was not found in the database.", vbExclamation
  Else
    Me.Form.Bookmark = rst.Bookmark
    Me.txtDealerName.SetFocus
  End If
  rst.Close
  Set rst = Nothing

Exit Function
errHandler:
ErrHand Me.Name, "FindOrder"
Exit Function
End Function