Private Sub Form_Load()
On Error GoTo errHandler

  Me.ServerFilter = ""
  iAdvertAllocPage = Me.tabDealer.Pages("pagAdvertAlloc").PageIndex
  iDealerGoalsPage = Me.tabDealer.Pages("pagDealerGoals").PageIndex
  

  Me.ServerFilter = ""

  If bTCMode Then
    Me.Caption = "Tom Cat Dealers"
    Me.RecordSource = "SELECT * FROM dbo.tblAllDealers WHERE DealerType = 1"
  Else
    Me.Caption = "Factory Cat Dealers"
    Me.RecordSource = "SELECT * FROM dbo.tblAllDealers WHERE DealerType = 0"
  End If
  
  mstrGoalYear = CStr(Year(Date) + 1)

Exit Sub
errHandler:
ErrHand Me.Name, "Form_Load"
Exit Sub
End Sub

