Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database   'Use database order for string comparisons
Option Explicit

Private Sub Report_NoData(Cancel As Integer)

  MsgBox "The criteria you specified did not find any leads.", vbExclamation
  Cancel = True
  Exit Sub
  
End Sub

Private Sub Report_Open(Cancel As Integer)
On Error GoTo errHandler

  Dim fR As New Form_ldfrptLeadReport
  If mbTomCat Then
    Me.Caption = "TC Leads"
    fR.comDealerName.RowSource = "SELECT DealerName FROM dbo.tblAllDealers WHERE (CurrentDealer = 1) AND (DealerType = 1) ORDER BY DealerName"
    Me.lblHeading.Caption = "Tom Cat " & Me.lblHeading.Caption
    SetSPVar "LeadType", 2
  ElseIf mbFCPurchased Then
    Me.Caption = "Pur Leads"
    fR.comDealerName.RowSource = "SELECT DealerName FROM dbo.tblAllDealers WHERE (CurrentDealer = 1) ORDER BY DealerName"
    Me.lblHeading.Caption = "Purchased " & Me.lblHeading.Caption
    SetSPVar "LeadType", 1
  Else
    Me.Caption = "FC Leads"
    fR.comDealerName.RowSource = "SELECT DealerName FROM dbo.tblAllDealers WHERE (CurrentDealer = 1) AND (DealerType = 0) ORDER BY DealerName"
    Me.lblHeading.Caption = "Factory Cat " & Me.lblHeading.Caption
    SetSPVar "LeadType", 0
  End If
  fR.Caption = Me.Caption
  fR.Visible = True
  Pause fR
  If fR.blnCancel Then
    Cancel = True
    Set fR = Nothing
    Exit Sub
  End If
  
  If fR.chkPageBreak Then
    Me.DealerNameHeader.ForceNewPage = 1 'before section
  Else
    Me.DealerNameHeader.ForceNewPage = 0 'no break
  End If
  
  SetSPVar "FromDate", fR.datFrom
  SetSPVar "ToDate", fR.datTo
  SetSPVar "ActiveInactive", fR.txtActiveInactive
  SetSPVar "DealerName", fR.comDealerName
  SetSPVar "CompanyName", fR.txtCompanyName
  SetSPVar "ResponseMethod", fR.txtResponseMethod

  Dim strCriteria As String
  
  strCriteria = FormatFromTo(fR.datFrom, fR.datTo)
  If fR.comDealerName <> "" Then
    If strCriteria <> "" Then strCriteria = strCriteria & ", "
    strCriteria = strCriteria & "Dealer = " & fR.comDealerName
  End If
  If fR.txtActiveInactive <> "" Then
    If strCriteria <> "" Then strCriteria = strCriteria & ", "
    If fR.txtActiveInactive = "I" Then
      strCriteria = strCriteria & "Inactive Leads"
    Else
      strCriteria = strCriteria & "Active Leads"
    End If
  End If
  If fR.txtResponseMethod <> "" Then
    If strCriteria <> "" Then strCriteria = strCriteria & ", "
    strCriteria = strCriteria & "Response Method = " & fR.txtResponseMethod
  End If
  Me.lblCriteria.Caption = strCriteria
  
  Set fR = Nothing

Exit Sub
errHandler:
ErrHand Me.Name, "Report_Open"
Exit Sub
End Sub