Private Sub cmdPrintByCounty_Click()
On Error Resume Next

    Dim DocName As String, strCriteria As String
        
    If IsNull(Me.txtState) Then
        MsgBox "You must enter a state name to use the Print by County report.", vbExclamation
        Me.txtState.SetFocus
        Exit Sub
    End If
    
    DoCmd.OpenForm "frptDealerProspectCounty", , , , , acDialog
    If SysCmd(acSysCmdGetObjectState, acForm, "frptDealerProspectCounty") <> acObjStateOpen Then
        Exit Sub
    End If
    
    Me.Visible = False
    
    If Not IsNull(Me.txtState) Then
        strCriteria = "[MailState] = '" & Me.txtState & "'"
    End If
        
    If Not IsNull(Forms![frptDealerProspectCounty]![comCounty]) Then
        If strCriteria <> "" Then strCriteria = strCriteria & " And "
        strCriteria = strCriteria & "[MailCounty] = '" & Forms![frptDealerProspectCounty]![comCounty] & "'"
    End If

    
    DocName = "rptDealerProspectByCounty"
    DoCmd.OpenReport DocName, acViewPreview, , strCriteria
    

End Sub

