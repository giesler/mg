Private Sub btnContactRpt_Click()
On Error GoTo Err_btnContactRpt_Click

    Dim DocName As String, strCriteria As String
    
    Me.Visible = False
    DocName = "rptDealerProspectByCompany"
    If Not IsNull(Me.txtState) Then
        strCriteria = "[MailState] = '" & Me.txtState & "'"
    End If
        
    OpenWaitForm "Opening company name report..."
    DoCmd.OpenReport DocName, acViewPreview, , strCriteria
    CloseWaitForm
    
Exit_btnContactRpt_Click:
    Exit Sub

Err_btnContactRpt_Click:
    MsgBox Error$
    CloseWaitForm
    Resume Exit_btnContactRpt_Click
    
End Sub

