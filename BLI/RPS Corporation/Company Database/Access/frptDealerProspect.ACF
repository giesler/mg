Private Sub btnAkg_Click()
On Error GoTo Err_btnAkg_Click

    Dim DocName As String
    Dim Criteria As String
    DocName = "Rpt_Acknowledgment"
    
    DoCmd.Close A_FORM, "Frm_ReportMenu"
    Criteria = InputBox("Enter the order number:")
    DoCmd.OpenReport DocName, A_PREVIEW, , "[Order Number] = """ & Criteria & """"

Exit_btnAkg_Click:
    Exit Sub

Err_btnAkg_Click:
    MsgBox Error$
    Resume Exit_btnAkg_Click
    
End Sub

