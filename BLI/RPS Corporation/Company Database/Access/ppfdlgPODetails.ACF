CodeBehindForm
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database
Option Explicit

Public lPOID As Long
Public lPOPartID As Long
Public lPOPartDetailID As Long

Private Sub cmdClose_Click()
Me.Visible = False
End Sub

Private Sub Form_Open(Cancel As Integer)
On Error GoTo Form_Open_Err

Exit Sub
Form_Open_Err:
ErrHand Me.Name, "Form_Open"
Exit Sub
End Sub

Public Sub FillParts(lPOIDin As Long, lPOPartIDin As Long, lPOPartDetailIDin As Long)
On Error GoTo FillParts_Err
lPOID = lPOIDin
lPOPartID = lPOPartIDin
lPOPartDetailID = lPOPartDetailIDin

If lPOID = 0 Then Exit Sub
Me.Caption = "PO #" & Nz(lPOID, "")

Dim sSQL As String, li As ListItem, r As Recordset

sSQL = "select pp.POPartID, pp.RPSPartNum, pp.VendorPartNumber, pp.PartDescription " _
        & "from dbo.tblPOPart pp, dbo.tblParts pa where pp.RPSPartNum = pa.RPSPartNum and " _
        & "fkPOID = " & lPOID & " order by pa.RPSPNSort"
Set r = New Recordset
r.Open sSQL, CurrentProject.Connection, adOpenForwardOnly, adLockReadOnly

With r
  Do While Not .EOF
    Set li = Me.lvParts.ListItems.Add(, "pp:" & !POPartID, Nz(!VendorPartNumber, ""))
    li.SubItems(1) = Nz(!RPSPartNum, "")
    li.SubItems(2) = Nz(!PartDescription, "")
    If !POPartID = lPOPartID Then li.Selected = True
    .MoveNext
  Loop
  .Close
End With
Set r = Nothing
FillOrders

Exit Sub
FillParts_Err:
ErrHand Me.Name, "FillParts"
Exit Sub
End Sub

Public Sub FillOrders()
On Error GoTo FillOrders_Err

If lPOID = 0 Then Exit Sub
Me.Caption = "PO #" & Nz(lPOID, "")

Dim sSQL As String, li As ListItem, r As Recordset
Set li = Me.lvParts.SelectedItem
If li Is Nothing Then Exit Sub

sSQL = "select Quantity, RequestedShipDate, ReceivedDate, QuantityReceived, CostEach, Value, " _
        & "Notes, POPartDetailID from dbo.tblPOPartDetail where fkPOPartID = " & Val(Mid(li.Key, 4)) _
        & "order by RequestedShipDate"
Set r = New Recordset
r.Open sSQL, CurrentProject.Connection, adOpenForwardOnly, adLockReadOnly

Me.lvOrders.ListItems.Clear
With r
  Do While Not .EOF
    Set li = Me.lvOrders.ListItems.Add(, "pd:" & !POPartDetailID, Format(Nz(!CostEach, 0), "Currency"))
    li.SubItems(1) = Nz(!Quantity, "")
    li.SubItems(2) = Format(Nz(!Value, ""), "Currency")
    li.SubItems(3) = Nz(!RequestedShipDate, "")
    li.SubItems(4) = Nz(!ReceivedDate, "")
    li.SubItems(5) = Nz(!QuantityReceived, "")
    li.SubItems(6) = Nz(!Notes, "")
    If !POPartDetailID = lPOPartDetailID Then li.Selected = True
    .MoveNext
  Loop
  .Close
End With
Set r = Nothing

Exit Sub
FillOrders_Err:
ErrHand Me.Name, "FillOrders"
Exit Sub
End Sub

Private Sub lvParts_Click()
FillOrders
End Sub