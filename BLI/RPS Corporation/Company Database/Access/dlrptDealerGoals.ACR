Private Sub Report_Open(Cancel As Integer)

Dim Criteria As String

Criteria = InputBox("Enter the year you want to print dealer goals for:", "Dealer Goals Report", VBA.Year(Now))
If Criteria = "" Then
    Cancel = True
    Exit Sub
End If

Me.Filter = "[Year] = '" & Criteria & "'"
Me.FilterOn = True

End Sub