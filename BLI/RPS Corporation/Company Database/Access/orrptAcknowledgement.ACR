Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database

Private Sub Report_Open(Cancel As Integer)
On Error GoTo errorHandler

  Dim fR As New Form_orfrptAcknowledgement
  Dim strFilter As String
  If mbTomCat Then
    iType = 1
    fR.Caption = "Tom Cat Acknowledgement Report"
    Me.lblHead.Caption = "Tom Cat Machine Order Confirmation"
    fR.comOrderID.RowSource = "SELECT OrderID, OrderNumber, OrderDate, Dealer " _
      & "FROM tblAllOrders WHERE OrderType = 1 ORDER BY tblOrders.OrderNumber"
  Else
    iType = 0
    fR.Caption = "FC Acknowledgement Report"
    Me.lblHead.Caption = "Factory Cat Machine Order Confirmation"
    fR.comOrderID.RowSource = "SELECT OrderID, OrderNumber, OrderDate, Dealer " _
      & "FROM tblAllOrders WHERE OrderType = 0 ORDER BY tblOrders.OrderNumber"
  End If

  fR.Visible = True
  Pause fR
  If fR.blnCancel Then
    Cancel = True
    Set fR = Nothing
    Exit Sub
  End If

  sOrderID = fR.comOrderID
  datFromDate = fR.datFrom
  datToDate = fR.datTo
  Me.lblPreparedBy.Caption = Nz(fR.comPreparedBy, "Irene Mopps")

Exit Sub
errorHandler:
ErrHand Me.Name, "Report_Open"
Exit Sub
End Sub