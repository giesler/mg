Private Sub Report_Open(Cancel As Integer)

Dim sMsg As String
sMsg = "Include only active prospects on this report?"

If MsgBox(sMsg, vbQuestion + vbYesNo, Me.Caption) = vbYes Then
  Me.ServerFilter = "Active = 1"
Else
  Me.ServerFilter = ""
End If
  
