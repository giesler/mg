End Sub

Private Sub FillLaborEntries()
On Error GoTo FillLaborEntries_Err

Dim li As ListItem, lsi As ListSubItem, sSQL As String, r As New Recordset, ch As ColumnHeader

Set ch = Me.lvItems.ColumnHeaders.Add(, , "Salesman", 1000, lvwColumnLeft)
Set ch = Me.lvItems.ColumnHeaders.Add(, , "Date", 1000, lvwColumnCenter)
Set ch = Me.lvItems.ColumnHeaders.Add(, , "Hours", 900, lvwColumnCenter)
Set ch = Me.lvItems.ColumnHeaders.Add(, , "Description", 5500, lvwColumnLeft)
Set ch = Me.lvItems.ColumnHeaders.Add(, , "SortOrder", 0, lvwColumnLeft)
Me.lvItems.Sorted = True
Me.lvItems.SortKey = 4
Me.lvItems.SortOrder = lvwDescending

Set li = Me.lvItems.ListItems.Add(, "e:add", "<add>")
PadListRow li
GreenListRow li
li.SubItems(4) = "99999999"

sSQL = "SELECT tblLaborLog.apkLLog, tblEmp.EmpName, tblLaborLog.LLogDate, tblLaborLog.LLogHours, "
sSQL = sSQL & "tblLaborLog.LLogDesc "
sSQL = sSQL & "FROM tblLaborLog INNER JOIN tblEmp ON tblLaborLog.afkLLogSM = tblEmp.apkEmp "
sSQL = sSQL & "WHERE tblLaborLog.afkLLogLabor = " & Nz(Me.apkLabor, 0)

r.Open sSQL, CurrentProject.Connection, adOpenForwardOnly, adLockReadOnly
Do While Not r.EOF
  Set li = Me.lvItems.ListItems.Add(, "e:" & r!apkLLog, Nz(r!EmpName, ""))
  li.SubItems(1) = Format(r!LLogDate, "Short Date")
  li.SubItems(2) = Nz(r!LLogHours, 0)
  li.SubItems(3) = CutStringAtEnter(Nz(r!LLogDesc, ""))
  li.SubItems(4) = CDateToStr(r!LLogDate)
  r.MoveNext
Loop
r.Close

Exit Sub
FillLaborEntries_Err:
ErrHand Me.Name, "FillLaborEntries"
Exit Sub
End Sub

Private Sub liEditLogEntry(blnAdd As Boolean)
On Error GoTo liEditLogEntry_Err

Dim li As ListItem, id As Long, fE As New Form_fdlgLaborLog
Dim r As New Recordset, blnFoundMain As Boolean, oLI As ListItem
Set li = Me.lvItems.SelectedItem
If li Is Nothing Then Exit Sub
If blnAdd Then
  r.Open "tblLaborLog", CurrentProject.Connection, adOpenDynamic, adLockOptimistic
  r.AddNew
  r!afkLLogLabor = Me.apkLabor
  r!afkLLogSM = CurUserID
  r!LLogDate = Date
  r.Update
  id = r!apkLLog
  r.Close
Else
  id = Val(Mid(li.key, 3))
End If
fE.Filter = "apkLLog = " & id
fE.FilterOn = True
Pause fE
  
If blnAdd And fE.blnCancel Then
  CurrentProject.Connection.Execute "DELETE FROM tblLaborLog WHERE apkLLog = " & id
End If

If Not fE.blnCancel Then
  If Not blnAdd Then Me.txtUsed = Me.txtUsed - Val(li.SubItems(2))
  Me.txtUsed = Me.txtUsed + fE.txtLLogHours
  Me.txtRemaining = Me.txtPurchased - Me.txtUsed
  If blnAdd Then Set li = Me.lvItems.ListItems.Add(, "e:" & id)
  li.Text = Nz(fE.comafkLLogSM.Column(1), "")
  li.SubItems(1) = Format(fE.txtLLogDate, "Short Date")
  li.SubItems(2) = Nz(fE.txtLLogHours, 0)
  li.SubItems(3) = CutStringAtEnter(Nz(fE.txtLLogDesc, ""))
  li.SubItems(4) = CDateToStr(fE.txtLLogDate)
End If

Exit Sub
liEditLogEntry_Err:
ErrHand Me.Name, "liEditLogEntry"
Exit Sub
End Sub

Private Sub FillPurchases()
On Error GoTo FillPurchases_Err

Dim li As ListItem, lsi As ListSubItem, sSQL As String, r As New Recordset, ch As ColumnHeader

Set ch = Me.lvItems.ColumnHeaders.Add(, , "Salesman", 1000, lvwColumnLeft)
Set ch = Me.lvItems.ColumnHeaders.Add(, , "Date", 1000, lvwColumnCenter)
Set ch = Me.lvItems.ColumnHeaders.Add(, , "Cost", 1000, lvwColumnRight)
Set ch = Me.lvItems.ColumnHeaders.Add(, , "Hours", 1000, lvwColumnRight)
Set ch = Me.lvItems.ColumnHeaders.Add(, , "Cost/Hour", 1200, lvwColumnRight)
Set ch = Me.lvItems.ColumnHeaders.Add(, , "Invoice", 1200, lvwColumnLeft)
Set ch = Me.lvItems.ColumnHeaders.Add(, , "SortOrder", 0, lvwColumnLeft)
Me.lvItems.Sorted = True
Me.lvItems.SortKey = 6
Me.lvItems.SortOrder = lvwDescending

Set li = Me.lvItems.ListItems.Add(, "e:add", "<add>")
li.SubItems(2) = " "
li.SubItems(3) = " "
li.SubItems(4) = " "
li.SubItems(6) = "99999999"
GreenListRow li

sSQL = "SELECT tblLaborPur.apkLPur, tblEmp.EmpName, tblLaborPur.LPurDate, tblInvoice.InvNumber, "
sSQL = sSQL & "tblLaborPur.LPurHours, tblLaborPur.LPurCost "
sSQL = sSQL & "FROM tblLaborPur INNER JOIN tblEmp ON "
sSQL = sSQL & "tblLaborPur.afkLPurSM = tblEmp.apkEmp LEFT OUTER JOIN tblInvoice ON tblLaborPur.afkLPurInv = tblInvoice.apkInv "
sSQL = sSQL & "WHERE tblLaborPur.afkLPurLabor = " & Nz(Me.apkLabor, 0)

r.Open sSQL, CurrentProject.Connection, adOpenForwardOnly, adLockReadOnly
Do While Not r.EOF
  Set li = Me.lvItems.ListItems.Add(, "p:" & r!apkLPur, Nz(r!EmpName, ""))
  li.SubItems(1) = Format(r!LPurDate, "Short Date")
  li.SubItems(2) = Format(r!LPurCost, "Currency")
  li.SubItems(3) = Nz(r!LPurHours, 0)
  li.SubItems(4) = Format(r!LPurCost / r!LPurHours, "Currency")
  li.SubItems(5) = Nz(r!InvNumber, "")
  li.SubItems(6) = CDateToStr(r!LPurDate)
  r.MoveNext
Loop
r.Close

Exit Sub
FillPurchases_Err:
ErrHand Me.Name, "FillPurchases_Err"
Exit Sub
End Sub

