If Not IsNull(Me.afkLaborCust) Then
  If MsgBox("Are you sure you want to change the customer on this labor sheet?", vbQuestion + vbYesNo + vbDefaultButton2) = vbNo Then Exit Sub
  blnChange = True
End If

lngCustID = SelectCustomer

If lngCustID <> 0 Then
  Me.afkLaborCust = lngCustID
