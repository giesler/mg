Private Sub Report_NoData(Cancel As Integer)
MsgBox "There were no invoices for the criteria entered: " & vbCrLf & Me.lblCriteria.Caption, vbExclamation
Cancel = True
End Sub


Private Sub Report_Open(Cancel As Integer)
On Error GoTo errHandler

  Dim fC As Form_frptInvoiceSummary, strCrit As String
  Set fC = New Form_frptInvoiceSummary
  fC.Caption = Me.Caption
  fC.lblDate.Caption = "Ship Date:"
  
  Pause fC

  If fC.mblnCancel Then
    Cancel = True
  Else
    SetSPVar "ShipFromDate", fC.datFrom
    SetSPVar "ShipToDate", fC.datTo
    SetSPVar "SoldName", fC.cmbSoldName
    SetSPVar "Classification", fC.cmbClassification
    SetSPVar "ReferenceNumber", fC.txtRefNum
    strCrit = FormatFromTo(fC.datFrom, fC.datTo)
    If strCrit <> "" Then strCrit = "Ship Date " + strCrit
    If fC.cmbSoldName <> "" Then
      If strCrit <> "" Then strCrit = strCrit + ", "
      strCrit = strCrit + "Customer: " + fC.cmbSoldName
    End If
    If fC.cmbClassification <> "" Then
      If strCrit <> "" Then strCrit = strCrit + ", "
      strCrit = strCrit + "Classification: " + fC.cmbClassification
    End If
    If fC.txtRefNum <> "" Then
      If strCrit <> "" Then strCrit = strCrit + ", "
      strCrit = strCrit + "Reference #: " + fC.txtRefNum
    End If
    Me.lblCriteria.Caption = strCrit
  End If
  
  Set fC = Nothing

Exit Sub
errHandler:
ErrHand Me.Name, "Report_Open"
Exit Sub
End Sub