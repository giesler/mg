Private Sub comJumpToCust_Click()

If IsNull(Me.comJumpToCust) Then Exit Sub

Dim r As ADODB.Recordset
Set r = Me.subSoldTo.Form.RecordsetClone
r.Find "apkSold = " & Me.comJumpToCust
Me.subSoldTo.Form.Bookmark = r.Bookmark
Set r = Nothing

End Sub