Private Sub Report_NoData(Cancel As Integer)

  MsgBox "There is no matching data.", vbExclamation, Me.Caption
  Cancel = True
  
End Sub

Private Sub Report_Open(Cancel As Integer)
On Error GoTo Report_Open_Err

  DoCmd.OpenForm "fdlgActiveHide", , , , , acDialog, Me.Caption
  If SysCmd(acSysCmdGetObjectState, acForm, "fdlgActiveHide") <> acObjStateOpen Then
    Cancel = True
    Exit Sub
  End If
  
  Dim f As Form_fdlgActiveHide, sFilt As String, sMsg As String
  Set f = Forms("fdlgActiveHide")
  
  Select Case f.comActive
    Case "Active and Inactive"
      SetSPVar "Active", Null
    Case "Active Only"
      SetSPVar "Active", "1"
    Case "Inactive Only"
      SetSPVar "Active", "0"
  End Select
  
  Select Case f.comHidden
    Case "Hidden and Not Hidden"
      SetSPVar "Hide", Null
    Case "Hidden Only"
      SetSPVar "Hide", "1"
    Case "Not Hidden Only"
      SetSPVar "Hide", "0"
  End Select
  
  SetSPVar "CustID", f.comCustomer.Column(0)
  If f.comCustomer <> "" Then
    Me.lblCriteria.Caption = f.comCustomer.Column(1)
  End If
  
  Set f = Nothing
  DoCmd.Close acForm, "fdlgActiveHide"

Exit Sub
Report_Open_Err:
ErrHand Me.Name, "Report_Open"
Exit Sub
End Sub