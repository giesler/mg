Private Sub Report_Open(Cancel As Integer)
On Error GoTo errHandler

  Dim fC As Form_frptShipmentConfirmation, strCrit As String
  Set fC = New Form_frptShipmentConfirmation
  fC.Caption = Me.Caption
  
  Pause fC

  If fC.mblnCancel Then
    Cancel = True
  Else
    SetSPVar "PO", fC.txtPO
  End If
  
  Set fC = Nothing

Exit Sub
errHandler:
ErrHand Me.Name, "Report_Open"
Exit Sub
End Sub