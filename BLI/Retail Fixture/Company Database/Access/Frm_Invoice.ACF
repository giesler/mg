CodeBehindForm
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database   'Use database order for string comparisons

Private mblnInitInv As Boolean    ' flag to track if we are in InitPO function already
Private mlngJumpToID As Long      ' jump to this ID when recordset loaded
Private mblnLoading As Boolean

Private Sub cmdDeleteInv_Click()
On Error GoTo errHandler

  ' make sure saved
  If Me.Dirty Then DoCmd.RunCommand acCmdSaveRecord
  
  If MsgBox("Are you sure you want to delete this invoice?  Items will be readded to inventory.", vbQuestion + vbYesNo + vbDefaultButton2) = vbYes Then
    If DeleteInvoice(Me.txtID) = True Then
      Set rs = Me.RecordsetClone
      rs.Find "ID = " + CStr(Me.txtID)
      If Not rs.BOF And Not rs.EOF Then
        rs.MovePrevious
        If Not rs.EOF Then lngPrevID = rs!ID
      End If
      Set rs = Nothing
      mlngJumpToID = lngPrevID
      mblnLoading = True
      Me.Refresh
    End If
  End If
  
Exit Sub
errHandler:
ErrHand Me.Name, "DeleteInv"
Exit Sub
End Sub

Private Sub cmdPrint_Click()
On Error GoTo errHandler
  
  Dim varTemp As Variant, intTemp As Integer
  
  ' save record if needed
  If Me.Dirty Then DoCmd.RunCommand acCmdSaveRecord
  
  If MsgBox("Would you like to reprint the invoice?", vbQuestion + vbYesNo) = vbYes Then
    If flgDebug Then
      PrintInvoice Me!ID, acViewPreview
    Else
      PrintInvoice Me!ID, acViewNormal
    End If
  End If
  
  If MsgBox("Would you like to reprint the labels for this invoice?", vbQuestion + vbYesNo) = vbYes Then
    intTemp = Nz(DLookup("[Labels]", "Tbl_PO", "ID=" + CStr(Me!ID)), 0)
    varTemp = Nz(InputBox("Enter the number of labels to print.  The number set when the PO was finished is shown.", "Labels", intLabels), 0)
    If varTemp = "" Then Exit Sub     ' if cancel pressed
    If Not IsNumeric(varTemp) Then
      MsgBox "You must enter a numeric value.", vbExclamation
      Exit Sub
    End If
    intTemp = Val(varTemp)
    If intTemp <> 0 Then
      If Not flgDebug Then
        OpenWaitForm "Printing labels..."
        varTemp = LabelText(Me.txtID)
        Call CSPrintLabel(CStr(varTemp), intTemp)
        Call CSStopDDE
        CloseWaitForm
      Else
        MsgBox CStr(intTemp) + " labels would be printing right now.", vbExclamation
      End If
    End If
  End If

Exit Sub
errHandler:
ErrHand Me.Name, "cmdPrint"
Exit Sub
End Sub


Private Sub Form_BeforeInsert(Cancel As Integer)
On Error GoTo errHandler

  ' Check if we are already in InitPO, if not we should run InitPO and cancel this insert
  Me.txtID = DMax("[id]", "Tbl_PO") + 1
  If Me.Dirty Then DoCmd.RunCommand acCmdSaveRecord
  If Screen.ActiveControl.Name <> "txtPO" Then
    MsgBox "You must enter a PO number.", vbExclamation
    Me.txtPO.SetFocus
    Cancel = True
  End If

Exit Sub
errHandler:
ErrHand Me.Name, "Form_BeforeInsert"
Exit Sub
End Sub

Private Sub Form_Current()
On Error GoTo errHandler

  Dim lngID As Long, rs As ADODB.Recordset, i As Long

  ' Reset flag just in case
  mblnInitInv = False
  If IsNull(Me.txtID) Then Me.txtPO.SetFocus
  
Exit Sub
errHandler:
ErrHand Me.Name, "Form_Current"
Exit Sub
End Sub

Private Sub Form_Deactivate()
On Error Resume Next
DoCmd.Close acForm, "fdlgFind"

End Sub

Private Sub Form_Load()
On Error GoTo errHandler

  If Me.ServerFilter <> "" Then Me.ServerFilter = ""
  If Me.Filter <> "" Then Me.Filter = ""
  If Me.FilterOn = True Then Me.FilterOn = False
  If Me.OpenArgs <> "" Then
    Me.Caption = Me.OpenArgs
    Me.lblHeading.Caption = Me.OpenArgs
  End If
  mblnLoading = True
  
Exit Sub
errHandler:
ErrHand Me.Name, "Form_Load"
Exit Sub
End Sub

Private Sub InitInv()
On Error GoTo errHandler

  ' check if we are already in init
  If mblnInitInv Then Exit Sub
    
  Dim fP As Form_fdlgPO
  Set fP = New Form_fdlgPO
  fP.Caption = "New Invoice"
  
  Pause fP
  
  If Not fP.mblnCancel Then
    mblnInitInv = True
    Me.txtPO = fP.txtPO
    Me.txtID = DMax("[id]", "Tbl_PO") + 1
    If Me.Dirty Then DoCmd.RunCommand acCmdSaveRecord
    mblnInitInv = False
    Me.txtDateOrdered.SetFocus
  End If

Exit Sub
errHandler:
ErrHand Me.Name, "InitInv"
Exit Sub
End Sub

Private Sub Form_Timer()
On Error GoTo errHandler

  Dim rs As ADODB.Recordset

  If mblnLoading Then
    Set rs = Me.RecordsetClone
    ' see if we are fetching records
    If rs.State = 9 Then
      Me.lblLoading.Caption = "Loading (" + CStr(rs.RecordCount) + ")..."
      Set rs = Nothing
      Exit Sub
    Else
      Me.lblLoading.Caption = ""
      mblnLoading = False
    End If
    Set rs = Nothing
  End If
  
Exit Sub
errHandler:
ErrHand Me.Name, "Form_Timer"
Exit Sub
End Sub

Private Sub txtPO_AfterUpdate()
On Error GoTo errHandler

  If IsNull(Me.txtID) Then
    Me.txtID = DMax("[id]", "Tbl_PO") + 1
    If Me.Dirty Then DoCmd.RunCommand acCmdSaveRecord
  End If
  
Exit Sub
errHandler:
ErrHand Me.Name, "txtPO_AfterUpdate"
Exit Sub
End Sub