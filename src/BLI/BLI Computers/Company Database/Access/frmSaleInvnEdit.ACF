End Sub

Private Sub lvInvn_ItemCheck(ByVal Item As Object)

Dim cLI As ListItem, sli As ListItem, gNode As Node, cIndex As Long
Dim cNode As Node, sSQL As String, sIndex As Long
Set cLI = Item
Set gNode = Me.tvInvn.SelectedItem

If cLI.Checked Then
  Set cNode = Me.tvInvn.SelectedItem
  sIndex = FindSelKeysIndex(cLI.key)
  If sIndex <> 0 Then
    Set sli = Me.lvSelInvn.ListItems(sIndex)
    sli.SubItems(1) = Val(Mid(cNode.key, 3))
    If InStr(sli.Tag, "saved") Then
      sli.SmallIcon = "barcode"
    Else
      sli.SmallIcon = "additem"
    End If
  Else
    Set sli = Me.lvSelInvn.ListItems.Add(, cLI.key, gNode.FullPath & "\" & cLI.Text, , "additem")
    sli.SubItems(1) = Val(Mid(gNode.key, 3))
    sli.SubItems(2) = cLI.Text
    sli.SubItems(3) = cLI.SubItems(1)
    sli.SubItems(4) = cLI.SubItems(2)
    If IsNull(Me.txtItemDesc) Then
      Me.txtItemDesc = cLI.Text
    End If
   End If
  UpdatePrices
Else
  Set cNode = Me.tvInvn.SelectedItem
  If FindSelKeysLI(cLI.key, sli) Then
    If sli.SmallIcon = "additem" Then
      Me.lvSelInvn.ListItems.Remove sli.Index
    Else
      sli.SmallIcon = "deleteitem"
    End If
  Else
    MsgBox "Error, key: " & cLI.key & ", not found", vbExclamation
  End If
  UpdatePrices
End If

End Sub

