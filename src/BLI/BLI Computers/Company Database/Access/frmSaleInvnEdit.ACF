Private Sub cmdItemInfo_Click()

Dim cLI As ListItem, id As Long, rInvn As New Recordset, sGrp As String
Dim sSQL As String

Set cLI = Me.lvSelInvn.SelectedItem
If Left(cLI.key, 1) = "s" Then
  Dim fr As New Form_frmSaleSubItemEdit
  fr.comafkSSItemGroup = cLI.SubItems(1)
  fr.txtSSItemDesc = cLI.SubItems(2)
  fr.txtSSItemCost = Val(cLI.SubItems(3))
  fr.txtSSItemPrice = Val(cLI.SubItems(4))
  fr.txtSSItemCost.Visible = Me.txtItemCost.Visible
  fr.Visible = True

  If InStr(Me.OpenArgs, "VIEW") Then
    fr.comafkSSItemGroup.Locked = True
    fr.txtSSItemCost.Locked = True
    fr.txtSSItemDesc.Locked = True
    fr.txtSSItemPrice.Locked = True
    Pause fr
  Else
    Pause fr
    If Not fr.blnCancel Then
      If cLI.SmallIcon = "barcode" Or cLI.SmallIcon = "deleteitem" Then
        cLI.SmallIcon = "updateditem"
      End If
      cLI.SubItems(1) = fr.comafkSSItemGroup
      cLI.SubItems(2) = fr.txtSSItemDesc
      cLI.SubItems(3) = fr.txtSSItemCost
      cLI.SubItems(4) = fr.txtSSItemPrice
      UpdatePrices
    End If
  End If
Else
  id = Val(Mid(cLI.key, 3))
  Dim fI As New Form_fdlgInvnInfo
  fI.Filter = "apkInvn = " & id
  fI.FilterOn = True
  If InStr(Me.OpenArgs, "VIEW") Then
    fI.RecordsetType = conSnapshot
    fI.AllowEdits = False
  End If
  Pause fI
  If Not fI.blnCancel Then
    cLI.Text = fI.comafkInvnGroup.Column(1) & "\" & fI.txtInvnDesc
    If cLI.SmallIcon = "barcode" Or cLI.SmallIcon = "deleteitem" Then
      cLI.SmallIcon = "updateditem"
    End If
    cLI.SubItems(1) = fI.comafkInvnGroup
    cLI.SubItems(2) = fI.txtInvnDesc
    cLI.SubItems(3) = fI.txtInvnCost
    cLI.SubItems(4) = 0
    UpdatePrices
  End If
End If

End Sub

Private Sub cmdOK_Click()
On Error GoTo cmdOK_Click_Err

Dim intOrder As Long

If InStr(Me.OpenArgs, "VIEW") Then
  Me.Visible = False
  Exit Sub
End If

If IsNull(Me.txtItemDesc) Then
    MsgBox "Please enter an item description!", vbExclamation
    Me.txtItemDesc.SetFocus
    Exit Sub
End If

If Me!SItemPrice = 0 Then
  Dim txtSugPrice, numHours As Single, txtMsg As String
  txtSugPrice = DLookup("[PItemSell]", "tblPrice", "[PItemDesc] = '" & Me.txtItemDesc & "'")
  If IsNull(txtSugPrice) Then
    Me.txtItemPrice = InputBox("Enter the price you want to charge:", "Enter Price", Me.txtItemPrice)
    If IsNull(Me.txtItemPrice) Then
      Me.txtItemPrice = 0
      Exit Sub
    End If
  Else
    numHours = DLookup("[PItemLaborTime]", "tblPrice", "[PItemDesc] = '" & Me.txtItemDesc & "'")
    txtMsg = "Enter the price you want to charge: " & vbCrLf & vbCrLf _
      & vbTab & "Regular sale price: " & vbTab & Format(txtSugPrice, "$#,##0.00") & vbCrLf
    ' check if there is an installed price
    If numHours <> 0 Then
      numHours = numHours * 47.5 + Val(txtSugPrice)
      txtMsg = txtMsg & vbTab & "Installed price:   " & vbTab & Format(numHours, "$#,##0.00") & vbCrLf
    End If
    ' check if we should show cost too
    If InStr(UCase(Me.OpenArgs), "COSTS") Then
      txtMsg = txtMsg & vbTab & "Cost of item(s):    " & vbTab & Format(Me!SItemCost, "$#,##0.00")
    End If
    Me.txtItemPrice = InputBox(txtMsg, "Enter Price", Val(txtSugPrice))
    If IsNull(Me.txtItemPrice) Then
      Me.txtItemPrice = 0
      Exit Sub
    End If
  End If
End If

Dim sli As ListItem, id As Long, sSQL As String

For Each sli In Me.lvSelInvn.ListItems
  id = Val(Mid(sli.key, 3))
  If sli.SmallIcon = "deleteitem" Then
    sSQL = "UPDATE tblInvn SET afkInvnSItem = 0 WHERE apkInvn = " & id
    CurrentProject.Connection.Execute sSQL
    sSQL = "DELETE FROM tblSaleSubItem WHERE apkSaleSubItem = " & Val(Mid(sli.Tag, InStr(sli.Tag, "ss:") + 3))
    CurrentProject.Connection.Execute sSQL
  ElseIf sli.SmallIcon = "additem" Or InStr(sli.Tag, "saved") = 0 Then
    sSQL = "INSERT INTO tblSaleSubItem (afkSaleSItem, afkSSItemGroup, SSItemDesc, "
    sSQL = sSQL & "SSItemCost, SSItemPrice, afkSSItemInvn) VALUES (" & Me!apkSItem & ", "
    sSQL = sSQL & Val(sli.SubItems(1)) & ", '" & sli.SubItems(2) & "', "
    sSQL = sSQL & Val(sli.SubItems(3)) & ", " & Val(sli.SubItems(4)) & ", " & id & ")"
    CurrentProject.Connection.Execute sSQL
    sSQL = "UPDATE tblInvn SET afkInvnSItem = " & Me!apkSItem & " WHERE apkInvn = " & id
    CurrentProject.Connection.Execute sSQL
  ElseIf sli.SmallIcon = "updateditem" Then
    sSQL = "UPDATE tblSaleSubItem SET SSItemDesc = '" & sli.SubItems(2) & "', "
    sSQL = sSQL & "afkSSItemGroup = " & sli.SubItems(1) & ", "
    sSQL = sSQL & "SSItemCost = " & Val(sli.SubItems(3)) & ", "
    sSQL = sSQL & "SSItemPrice = " & Val(sli.SubItems(4)) & " "
    sSQL = sSQL & "WHERE apkSaleSubItem = " & Val(Mid(sli.Tag, InStr(sli.Tag, "ss:") + 3))
    CurrentProject.Connection.Execute sSQL
  End If
Next sli

If Me!SItemWarn Then
  If DCount("apkSaleSubItem", "tblSaleSubItem", "afkSaleSItem = " & Me!apkSItem & " And afkSSItemInvn = 0") = 0 Then
    Me!SItemWarn = False
  End If
End If

UpdatePrices
blnCancel = False
Me.Visible = False

Exit Sub
cmdOK_Click_Err:
ErrHand Me.Name, "cmdOK"
Exit Sub

End Sub

Private Sub Form_Close()
On Error Resume Next
Me.ServerFilter = ""
End Sub

Private Sub Form_Current()
On Error GoTo Form_Current_Err

Dim rInvn As New Recordset, sSQL As String, li As ListItem, iInvnID As Long
Dim lsi As ListSubItem

sSQL = "SELECT tblSaleSubItem.apkSaleSubItem, tblInvnGrp.InvnGrpDesc, tblSaleSubItem.SSItemDesc, "
sSQL = sSQL & "tblSaleSubItem.afkSSItemGroup, tblSaleSubItem.SSItemCost, tblSaleSubItem.SSItemPrice, "
sSQL = sSQL & "tblSaleSubItem.afkSSItemInvn "
sSQL = sSQL & "FROM tblSaleSubItem INNER JOIN tblInvnGrp ON tblSaleSubItem.afkSSItemGroup = tblInvnGrp.apkInvnGrp "
sSQL = sSQL & "WHERE tblSaleSubItem.afkSaleSItem = " & Me!apkSItem
rInvn.Open sSQL, CurrentProject.Connection, adOpenForwardOnly, adLockReadOnly
Me.lvSelInvn.ListItems.Clear

Do While Not rInvn.EOF
  If rInvn!afkSSItemInvn <> 0 Then
    Set li = Me.lvSelInvn.ListItems.Add(, "i:" & rInvn!afkSSItemInvn, rInvn!InvnGrpDesc & "\" & rInvn!SSItemDesc, , "barcode")
    li.SubItems(1) = rInvn!afkSSItemGroup
    li.SubItems(2) = rInvn!SSItemDesc
    li.SubItems(3) = rInvn!SSItemCost
    li.SubItems(4) = rInvn!SSItemPrice
    li.Tag = "saved, ss:" & rInvn!apkSaleSubItem
    rInvn.MoveNext
  Else
    Set li = Me.lvSelInvn.ListItems.Add(, "s:" & rInvn!apkSaleSubItem, rInvn!InvnGrpDesc & "\" & rInvn!SSItemDesc, , "barcode_warn")
    li.SubItems(1) = rInvn!afkSSItemGroup
    li.SubItems(2) = rInvn!SSItemDesc
    li.SubItems(3) = rInvn!SSItemCost
    li.SubItems(4) = rInvn!SSItemPrice
    li.Tag = "saved, ss:" & rInvn!apkSaleSubItem
    RedListRow li
    rInvn.MoveNext
  End If
Loop
rInvn.Close

If Me!SItemWarn Then
  Me.lblWarn.Visible = True
Else
  Me.lblWarn.Visible = False
End If

Exit Sub
Form_Current_Err:
ErrHand Me.Name, "Form_Current"
Exit Sub

End Sub

Private Sub Form_Load()
On Error GoTo Form_Load_Err

If InStr(UCase(Me.OpenArgs), "VIEW") Then
  With Me
    .Caption = "View Sale Order Item"
    .AllowAdditions = False
    .AllowDeletions = False
    .AllowEdits = False
    .RecordsetType = conSnapshot
  End With
End If

If InStr(UCase(Me.OpenArgs), "COSTS") Then
  With Me
    .txtItemCost.Visible = True
  End With
End If

If InStr(UCase(Me.OpenArgs), "VIEW") Then
  Dim rNode As Node
  Me.lblItemSelPaid.Visible = True
  Me.lvInvn.Visible = False
  Me.tvInvn.Visible = False
  Me.lvInvn.Enabled = False
  Me.tvInvn.Enabled = False
  Me.cmdDeleteSItem.Enabled = False
Else
  'Load item selection stuff
  LoadInvnGrpTree
End If

If InStr(UCase(Me.OpenArgs), "COMPUTER") Then
  Dim pg As Page
  Set pg = Me.tabSItem.Pages("pagComputer")
  pg.Visible = True
  Set pg = Me.tabSItem.Pages("pagSubItems")
  Me.tabSItem.Value = pg.PageIndex
ElseIf InStr(UCase(Me.OpenArgs), "ADD") Then
  Me.tabSItem.Value = Me.tabSItem.Pages("pagItemSelection").PageIndex
End If

Exit Sub
Form_Load_Err:
ErrHand Me.Name, "Form_Load"
Exit Sub
End Sub

Public Function UpdatePrices()
On Error GoTo UpdatePrices_Err

Dim Cost As Currency, price As Currency, li As ListItem

Cost = 0
price = 0

For Each li In Me.lvSelInvn.ListItems
  If li.SmallIcon <> "deleteitem" Then
    Cost = Cost + Nz(li.SubItems(3), 0)
    price = price + Nz(li.SubItems(4), 0)
  End If
Next li

Me.txtItemCost = Cost
'Me.txtItemPrice = price
Me!SItemExtCost = Cost
Me!SItemExtPrice = Me!SItemPrice

Exit Function
UpdatePrices_Err:
ErrHand Me.Name, "UpdatePrices"
Exit Function

End Function

Private Sub lvInvn_DblClick()
