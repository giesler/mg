Private Sub cmdClear_Click()

If MsgBox("Are you sure you want to clear the debug messages?", vbQuestion + vbYesNo) = vbYes Then
  Me.lvDebug.ListItems.Clear
End If

End Sub

Private Sub cmdClose_Click()

If MsgBox("Do you want to disable debug messages?", vbQuestion + vbYesNo) = vbYes Then
  flgDebug = False
End If
Me.Visible = False

End Sub

Private Sub cmdSave_Click()

If MsgBox("Save the debug messages to '" & GetDBDir & "\Debug Messages.txt'", vbQuestion + vbYesNo) = vbYes Then
  Dim li As ListItem, fN As Long
  fN = FreeFile
  Open GetDBDir & "\Debug Messages.txt" For Output As fN
  For Each li In Me.lvDebug.ListItems
    Print #fN, li.Text & " - " & li.SubItems(1) & ":  " & li.SubItems(2)
  Next li
  Close fN
End If

End Sub

Private Sub Form_Open(Cancel As Integer)
Me.lvDebug.Sorted = True
Me.lvDebug.SortOrder = lvwDescending
End Sub