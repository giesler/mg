Private Sub Report_Open(Cancel As Integer)
On Error GoTo Report_Open_Err

Dim fR As New Form_ppfrptProdReleases

fR.Visible = True
Pause fR
If fR.blnCancel Then
    Set fR = Nothing
    Cancel = True
    Exit Sub
End If

If fR.txtEndDate = "" Then fR.txtEndDate = Date

datToDate = fR.txtEndDate

Me.lblCrit.Caption = "Requested Ship Date before " & fR.txtEndDate

Set fR = Nothing

Exit Sub
Report_Open_Err:
ErrHand Me.Name, "Report-OPen"
Exit Sub

End Sub