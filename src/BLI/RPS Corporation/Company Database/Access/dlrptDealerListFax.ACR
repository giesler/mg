Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database
Option Explicit

Private Sub Report_NoData(Cancel As Integer)

  MsgBox "There were no dealers found.", vbExclamation
  Exit Sub
  
End Sub

Private Sub Report_Open(Cancel As Integer)

  If mbTomCat Then
    Me.lblHeading.Caption = "Tom Cat " & Me.lblHeading.Caption
    Me.Caption = "TC " & Me.Caption
    SetSPVar "DealerType", 1
  Else
    Me.lblHeading.Caption = "Factory Cat " & Me.lblHeading.Caption
    Me.Caption = "TC " & Me.Caption
    SetSPVar "DealerType", 0
  End If
  
End Sub