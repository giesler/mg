Option Compare Database

Private Sub Report_Open(Cancel As Integer)

Dim fR As New Form_frptEnterDealer
fR.Caption = "Dealer Demo Report"
fR.Visible = True
Pause fR
If fR.blnCancel Then
    Cancel = True
    Set fR = Nothing
    Exit Sub
End If

Dim strFilter As String

If fR.comDealerName <> "" Then
    strFilter = "DealerName = " & Chr(34) & fR.comDealerName & Chr(34)
End If

If strFilter <> "" Then
    Me.Filter = strFilter
    Me.FilterOn = True
Else
    Me.FilterOn = False
End If

Set fR = Nothing

End Sub