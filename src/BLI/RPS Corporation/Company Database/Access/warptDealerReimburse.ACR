CodeBehindForm
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database   'Use database order for string comparisons

Private Sub Report_Open(Cancel As Integer)

If mbTomCat Then
  iType = 1
  Me.Caption = "TC Warranty: Dealer Reimbursement"
  Me.lblHead.Caption = "Tom Cat Dealer Warranty Reimbursement"
Else
  iType = 0
  Me.Caption = "FC Warranty: Dealer Reimbursement"
  Me.lblHead.Caption = "Factory Cat Dealer Warranty Reimbursement"
End If

Dim fR As New Form_frptEnterDatesDealerWarranty
fR.Caption = Me.Caption
fR.lblDateCaption.Caption = "Date of failure"
fR.lstOpenWarranty.Selected(2) = True
fR.Visible = True
Pause fR
If fR.blnCancel Then
    Cancel = True
    Set fR = Nothing
    Exit Sub
End If

Dim strMsg As String

datFromDate = fR.datFrom
datToDate = fR.datTo
sDealerName = fR.comDealer
iOpenFlag = fR.lstOpenWarranty

If fR.comDealer <> "" Then
    strMsg = "Dealer: " & fR.comDealer
End If

If fR.datFrom <> "" Or fR.datTo <> "" Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & FormatFromTo(fR.datFrom, fR.datTo)
End If

If IsNull(fR.lstOpenWarranty) Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & "Open and closed claims"
ElseIf fR.lstOpenWarranty = 0 Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & "Closed claims"
ElseIf fR.lstOpenWarranty = 1 Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & "Open claims"
End If

If strMsg <> "" Then
    Me.lblHeading.Caption = strMsg
End If

Set fR = Nothing


End Sub