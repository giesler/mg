CodeBehindForm
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database   'Use database order for string comparisons

Private Sub Report_Open(Cancel As Integer)
On Error GoTo errorHandler

  If SecureAuthorize() = False Then
    Cancel = True
    Exit Sub
  End If

  Dim fR As New Form_frptEnterDates
  If mbTomCat Then
    Me.Caption = "TC Orders: Sales by Model Report"
    Me.lblHeading.Caption = "Tom Cat Sales by Model Report"
    iType = 1
  Else
    Me.Caption = "FC Orders: Sales by Model Report"
    Me.lblHeading.Caption = "Factory Cat Sales by Model Report"
    iType = 0
  End If
  fR.Caption = Me.Caption
  fR.lblDateCaption.Caption = "Order Date:"
  fR.Visible = True
  Pause fR
  If fR.blnCancel Then
    Cancel = True
    Set fR = Nothing
    Exit Sub
  End If

  datFromDate = fR.datFrom
  datToDate = fR.datTo
  
  If fR.datFrom <> "" Or fR.datTo <> "" Then
    Me.lblHeading.Caption = Me.lblHeading.Caption & " - " & FormatFromTo(fR.datFrom, fR.datTo)
  End If

  Set fR = Nothing

Exit Sub
errorHandler:
ErrHand Me.Name, "Report_Open"
Exit Sub
End Sub