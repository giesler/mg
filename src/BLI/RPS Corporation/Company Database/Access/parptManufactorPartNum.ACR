Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database

Private Sub Report_Open(Cancel As Integer)

Dim fR As New Form_frptEnterVendor
fR.Caption = "Manufacturer Part Number Listing"
fR.chkIncludeHiddenParts.Visible = True
fR.Visible = True
Pause fR
If fR.blnCancel Then
    Cancel = True
    Set fR = Nothing
    Exit Sub
End If

Dim strFilter As String

If fR.comVendorName <> "" Then
    strFilter = "[VendorName] = '" & fR.comVendorName & "'"
    Me.lblHeading.Caption = Me.lblHeading.Caption & " - " & fR.comVendorName
End If

If Not fR.chkIncludeHiddenParts Then
  If strFilter <> "" Then strFilter = strFilter & " AND "
  strFilter = strFilter & "[HideOnReports] = 0"
End If

If strFilter <> "" Then
    Me.ServerFilter = strFilter
Else
    Me.ServerFilter = ""
End If

Set fR = Nothing

End Sub