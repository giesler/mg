Private Function EditSubPart(flgAdd As Boolean)
On Error GoTo EditSubPart_Err

Dim li As ListItem, fD As Form_pafdlgSubPart, sSQL As String, ID As Long
Dim cTotal As Currency
Set li = Me.lvSubParts.SelectedItem
If Not flgAdd And li Is Nothing Then Exit Function

If IsNull(Me!PartID) Then DoCmd.RunCommand acCmdSaveRecord

Set fD = New Form_pafdlgSubPart
If flgAdd Then
  Dim r As New Recordset
  r.Open "dbo.tblPartsSubParts", CurrentProject.Connection, adOpenDynamic, adLockOptimistic
  r.AddNew
  r!fkPartID = Me!PartID
  r.Update
  ID = r!SubPartID
Else
  fD.comNumber = li.Text
  fD.txtDescription = li.SubItems(1)
  fD.txtSource = li.SubItems(2)
  fD.txtSourcePN = li.SubItems(3)
  fD.txtCost = li.SubItems(4)
  fD.txtQty = li.SubItems(5)
  fD.txtExtCost = li.SubItems(6)
End If

fD.Caption = Nz(Me.txtRPSPartNum & " Sub Part", fD.Caption)
Pause fD

If Not fD.blnCancel Then
  sSQL = "update dbo.tblPartsSubParts set SubNum = '" & QuoteName(fD.comNumber) & "', SubDescription = '" & QuoteName(fD.txtDescription) & "', SubSource = '" & QuoteName(fD.txtSource) & "', "
  sSQL = sSQL & "SubSourcePartNum = '" & QuoteName(fD.txtSourcePN) & "', SubCost = " & Val(Nz(fD.txtCost, 0)) & ", SubQty = " & Nz(fD.txtQty, 0) & ", SubExtCost = " & Val(Nz(fD.txtExtCost, 0))
  If flgAdd Then
    sSQL = sSQL & " where SubPartID = " & ID
  Else
    sSQL = sSQL & " where SubPartID = " & li.SubItems(7)
  End If
  
  CurrentProject.Connection.Execute sSQL
  If flgAdd Then Set li = Me.lvSubParts.ListItems.Add(, , Nz(fD.comNumber, ""))
  li.Text = Nz(fD.comNumber, "")
  li.SubItems(1) = Nz(fD.txtDescription, "")
  li.SubItems(2) = Nz(fD.txtSource, "")
  li.SubItems(3) = Nz(fD.txtSourcePN, "")
  li.SubItems(4) = Format(Nz(fD.txtCost, 0), "Currency")
  li.SubItems(5) = Nz(fD.txtQty, 0)
  li.SubItems(6) = Format(Nz(fD.txtExtCost, 0), "Currency")
  For Each li In Me.lvSubParts.ListItems
    cTotal = cTotal + li.SubItems(6)
  Next li
  Me.lblTotalCost.Caption = Format(cTotal, "Currency")
  Me.txtCostEach = cTotal
  If FabVendor(Me!VendorName) Then
    Me.txtDealerNet = DNetMultFab * Me.txtCostEach
  Else
    Me.txtDealerNet = DNetMult * Me.txtCostEach
  End If
  If Me.txtDealerNet < DNetMin And Me.txtDealerNet > 0 Then Me.txtDealerNet = DNetMin
  Me.txtSuggestedList = Me.txtDealerNet * SugListMult
  sSQL = "update tblPartsVendors set VendorCostEach = " & cTotal & " where VendorPrimary = 1 and PartID = " & Me!PartID
  CurrentProject.Connection.Execute sSQL
ElseIf flgAdd Then
  'delete record added
  CurrentProject.Connection.Execute "delete dbo.tblPartsSubParts where SubPartID = " & ID
End If

Set fD = Nothing

Exit Function
EditSubPart_Err:
ErrHand Me.Name, "EditSubPart"
Exit Function
End Function

