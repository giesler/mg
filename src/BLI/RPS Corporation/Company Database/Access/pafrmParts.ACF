On Error GoTo cmdFinishDelete_Err

Dim li As ListItem, sSQL As String, cst As Currency

Set li = Me.lvFinish.SelectedItem
If li Is Nothing Then
  MsgBox "You must select a finish to delete!", vbExclamation
  Exit Sub
End If

If MsgBox("Are you sure you want to delete the selected finish?", vbQuestion + vbYesNo) = vbYes Then
  sSQL = "delete dbo.tblPartsFinish where FinishID = " & Val(Mid(li.Key, 4))
  CurrentProject.Connection.Execute sSQL
  UpdateFinish
End If

cst = Nz(DLookup("VendorCostEach", "dbo.tblPartsVendors", "VendorCostEach IS NOT NULL and VendorPrimary = 1 and PartID = " & Me!PartID), 0) + Nz(DSum("FCost", "dbo.tblPartsFinish", "FCost IS NOT NULL and FOrder IS NOT NULL and PartID = " & Me!PartID), 0)
If cst <> Me.txtCostEach Then
  Me.txtCostEach = cst
  If FabVendor(Me!VendorName) Then
    Me.txtDealerNet = DNetMultFab * Me.txtCostEach
  Else
    Me.txtDealerNet = DNetMult * Me.txtCostEach
  End If
  If Me.txtDealerNet < DNetMin And Me.txtDealerNet > 0 Then Me.txtDealerNet = DNetMin
  Me.txtSuggestedList = Me.txtDealerNet * SugListMult
End If

Exit Sub
