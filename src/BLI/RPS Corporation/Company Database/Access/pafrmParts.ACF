Private Function VendorDialog(bAdd As Boolean)
On Error GoTo VendorDialog_Err

If IsNull(Me.txtRPSPartNum) Then
  MsgBox "You must enter a part number before changing vendor information.", vbExclamation
  Exit Function
End If

If IsNull(Me!PartID) Then DoCmd.RunCommand acCmdSaveRecord

Dim r As New Recordset, sSQL As String, li As ListItem, f As Form_pafdlgVendor
Set f = New Form_pafdlgVendor
f.Caption = Me!RPSPartNum

If Not bAdd Then
  Set li = Me.lvVendors.SelectedItem
  If li Is Nothing Then
    MsgBox "You must select a vendor to edit.", vbExclamation
    Exit Function
  End If

  sSQL = "select * from tblPartsVendors where PartVendorID = " & Val(Mid(li.Key, 3))
  r.Open sSQL, CurrentProject.Connection, adOpenForwardOnly, adLockReadOnly
  With f
    .comVendorID = r!VendorID
    .txtVendorCostEach = r!VendorCostEach
    .txtVendorPartName = r!VendorPartName
    .txtVendorPartNum = r!VendorPartNum
    .chkVendorPrimary = r!VendorPrimary
  End With
  r.Close
  Set r = Nothing
End If

If Me.lvVendors.ListItems.Count = 0 Then f.chkVendorPrimary = True

Pause f

If Not f.blnCancel Then
  If f.chkVendorPrimary Then
    ' clear other primary flags, set local fields
    CurrentProject.Connection.Execute "update tblPartsVendors set VendorPrimary = 0 where PartID = " & Me!PartID
    Me!VendorName = f.comVendorID.Column(1)
    Me!CostEach = Nz(f.txtVendorCostEach, 0) + Nz(DSum("FCost", "dbo.tblPartsFinish", "FCost IS NOT NULL and FOrder IS NOT NULL and PartID = " & Me!PartID), 0)
    Me!ManfPartNum = f.txtVendorPartNum
    Me!VendorPartName = f.txtVendorPartName
    If FabVendor(f.comVendorID.Column(1)) Then
      Me.txtDealerNet = DNetMultFab * Me.txtCostEach
    Else
      Me.txtDealerNet = DNetMult * Me.txtCostEach
    End If
    If Me.txtDealerNet < DNetMin And Me.txtDealerNet > 0 Then Me.txtDealerNet = DNetMin
    Me.txtSuggestedList = Me.txtDealerNet * SugListMult
  
  End If
  If bAdd Then
    sSQL = "insert tblPartsVendors (PartID, VendorID, VendorPartName, VendorPartNum, VendorCostEach, VendorPrimary) "
    sSQL = sSQL & "values (" & Me!PartID & ", " & f.comVendorID & ", '" & DQ(Nz(f.txtVendorPartName, "")) & "', "
    sSQL = sSQL & "'" & DQ(Nz(f.txtVendorPartNum, "")) & "', " & Nz(f.txtVendorCostEach, 0) & ", " & IIf(f.chkVendorPrimary, 1, 0) & ")"
  Else
    sSQL = "update tblPartsVendors set VendorID = " & f.comVendorID & ", "
    sSQL = sSQL & "VendorPartName = '" & DQ(f.txtVendorPartName) & "', "
    sSQL = sSQL & "VendorPartNum = '" & DQ(f.txtVendorPartNum) & "', "
    sSQL = sSQL & "VendorCostEach = " & Nz(f.txtVendorCostEach, 0) & ", "
    sSQL = sSQL & "VendorPrimary = " & IIf(f.chkVendorPrimary, 1, 0)
    sSQL = sSQL & " where PartVendorID = " & Val(Mid(li.Key, 3))
  End If
  Debug.Print sSQL
  CurrentProject.Connection.Execute sSQL
  UpdateVendors
End If

Exit Function
VendorDialog_Err:
ErrHand Me.Name, "VendorDialog"
Exit Function
End Function

