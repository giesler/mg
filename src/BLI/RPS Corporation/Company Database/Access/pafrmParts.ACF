Private Function UpdateRelSched()
On Error Resume Next
Me.lvRelSched.ListItems.Clear
If IsNull(Me.txtRPSPartNum) Then Exit Function

On Error GoTo UpdateRelSched_Err


Dim r As New Recordset, cmd As New Command, prm As New Parameter, li As ListItem
cmd.ActiveConnection = CurrentProject.Connection
cmd.CommandType = adCmdStoredProc
cmd.CommandText = "dbo.ppsprptVendorRelease"
prm.Name = "POID"
prm.Type = adVarChar
prm.Size = 40
prm.Value = Null
cmd.Parameters.Append prm
Set prm = New Parameter
prm.Name = "sRPSPartNum"
prm.Type = adVarChar
prm.Size = 40
prm.Value = Me.txtRPSPartNum
cmd.Parameters.Append prm
Set prm = New Parameter
prm.Name = "sVendorPartNum"
prm.Type = adVarChar
prm.Size = 40
prm.Value = Null
cmd.Parameters.Append prm
Set prm = New Parameter
prm.Name = "iOpenFlag"
prm.Type = adVarChar
prm.Size = 2
prm.Value = IIf(Nz(Me.optOnlyOpenOrders, 1), 1, 0)
cmd.Parameters.Append prm
Set r = cmd.Execute
Do While Not r.EOF
  Set li = Me.lvRelSched.ListItems.Add(, , r!POID)
  li.SubItems(1) = Nz(r!VendorPartNumber, "")
  li.SubItems(2) = Nz(r!PartDescription, "")
  li.SubItems(3) = Format(Nz(r!RequestedShipDate, ""), "m/d/yy")
  li.SubItems(4) = Nz(r!Quantity, "")
  li.SubItems(5) = Format(Nz(r!Value, "0"), "Currency")
  li.SubItems(6) = r!POPartID
  li.SubItems(7) = r!POPartDetailID
  r.MoveNext
Loop
r.Close
Set r = Nothing
Set cmd = Nothing

Exit Function
UpdateRelSched_Err:
ErrHand Me.Name, "UpdateRelSched"
Exit Function
End Function

