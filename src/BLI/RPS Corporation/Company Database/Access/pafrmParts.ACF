Public Function FindPart()
On Error GoTo FindPart_Err

Dim sPart As String

sPart = Application.CommandBars(Me.Toolbar).Controls("Find RP&S P/N:").Text
If IsNull(sPart) Or sPart = "" Then Exit Function

Dim rst As Recordset

Set rst = Me.RecordsetClone

rst.Find "[RPSPartNum] = '" & sPart & "'"
If rst.EOF Then
  MsgBox "The RPS part number '" & sPart & "' was not found in the database.", vbExclamation
Else
  Me.Form.Bookmark = rst.Bookmark
  Me.txtRPSPartNum.SetFocus
End If
rst.Close
Set rst = Nothing

Exit Function
FindPart_Err:
ErrHand Me.Name, "FindPart"
Exit Function
End Function

