Option Compare Database

Private Sub Report_Open(Cancel As Integer)

Dim fR As New Form_ppfrptReceivingReport

fR.Visible = True
Pause fR
If fR.blnCancel Then
    Set fR = Nothing
    Cancel = True
    Exit Sub
End If

Dim strCrit As String, strMsg As String

sVendorName = fR.comVendorName
sPOID = fR.comPO
iOpenFlag = fR.chkIncludeOnlyOpen

If fR.comVendorName <> "" Then
    strMsg = "Vendor " & fR.comVendorName
End If

If fR.comPO <> "" Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & "PO " & fR.comPO
End If

If fR.chkIncludeOnlyOpen Then
    If strMsg <> "" Then strMsg = strMsg & ", "
    strMsg = strMsg & "Open Orders"
End If

Select Case fR.optSort
    Case 1  'VendorPartNum
        Me.OrderBy = "[VendorPartNumber],[RequestedShipDate]"
        Me.OrderByOn = True
    Case 2  'RPSPartNum
        Me.OrderBy = "[RPSPartNum],[RequestedShipDate]"
        Me.OrderByOn = True
End Select

If strMsg <> "" Then
    Me.lblCrit.Caption = strMsg
End If

Set fR = Nothing
    
End Sub