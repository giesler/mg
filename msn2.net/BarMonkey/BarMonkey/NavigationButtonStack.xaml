<UserControl x:Class="msn2.net.BarMonkey.NavigationButtonStack"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ListBox Grid.Row="1" Grid.Column="2" Name="activityList" Background="Transparent" HorizontalContentAlignment="Stretch" BorderThickness="0">
        <ListBox.ItemTemplate>
            <DataTemplate>
                <Button Style="{DynamicResource DefaultListButtonStyle}" IsEnabled="{Binding Path=IsEnabled}" Name="selectActivity" Click="selectActivity_Click" Tag="{Binding Path=PageUrl}" />
            </DataTemplate>
        </ListBox.ItemTemplate>
    </ListBox>
</UserControl>
