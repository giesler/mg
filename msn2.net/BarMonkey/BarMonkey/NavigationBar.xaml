<UserControl x:Class="msn2.net.BarMonkey.NavigationBar"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    >
    <Grid>
        <Grid.Background>
            <SolidColorBrush Color="Silver" />
        </Grid.Background>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Button Grid.Column="0" Style="{DynamicResource EmptyButtonStyle}" Margin="10" Click="back_Click" x:Name="back">
            <Image Source="/Resources/appbar.back.rest.png" />
        </Button>
        <Button Grid.Column="1" Name="home" Content="HOME" Style="{DynamicResource EmptyButtonStyle}" Margin="10" Click="home_Click">
            
        </Button>
        <Button Grid.Column="2" Style="{DynamicResource EmptyButtonStyle}" Margin="10" IsEnabled="False" Click="next_Click" x:Name="next">
            <Image Source="/Resources/appbar.next.rest.png" />
        </Button>
    </Grid>
</UserControl>
