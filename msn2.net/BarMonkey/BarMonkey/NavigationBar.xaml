<UserControl x:Class="msn2.net.BarMonkey.NavigationBar"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    >
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Button Grid.Column="0" Name="back" Content="&lt; Back" Style="{DynamicResource DefaultButtonStyle}" Margin="10" Click="back_Click" />
        <Button Grid.Column="1" Name="home" Content="Home" Style="{DynamicResource DefaultButtonStyle}" Margin="10" Click="home_Click" />
        <Button Grid.Column="2" Name="next" Content="Next &gt;" Style="{DynamicResource DefaultButtonStyle}" Margin="10" IsEnabled="False" Click="next_Click" />
    </Grid>
</UserControl>
