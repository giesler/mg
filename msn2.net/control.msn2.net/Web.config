<configuration>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.6"/>
    <httpRuntime targetFramework="4.5"/>
    <customErrors mode="Off"/>
  </system.web>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IISY" maxReceivedMessageSize="6553600"/>
        <binding name="BasicHttpBinding_IDeviceControl" openTimeout="00:00:05" sendTimeout="00:00:05"/>
        <binding name="BasicHttpBinding_ISonos"/>
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://hs.msn2.net:8808/ISY.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IISY" contract="IsyData.IISY" name="BasicHttpBinding_IISY"/>
      <endpoint address="http://hs.msn2.net:8808/DeviceControl.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IDeviceControl" contract="DeviceControlService.IDeviceControl" name="BasicHttpBinding_IDeviceControl"/>
      <endpoint address="http://hs.msn2.net:8808/Sonos.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ISonos" contract="SonosService.ISonos" name="BasicHttpBinding_ISonos"/>
    </client>
  </system.serviceModel>
</configuration>